function FillPrice(obj, name, arrC, min_value, max_value, arr_text){ 
	var valuesArr;
	var i, j;
	var objOption;
	var changeField = obj.form.elements[name];
	n_max=changeField.value;
	/*if(obj.options[obj.selectedIndex].value==""){changeField.disabled = true;return;}*/
	
	//changeField.disabled = false;
	try	{ while(changeField.removeChild(changeField.firstChild)); }
	catch(e){}
	for(j=0; j<arrC.length; ++j){
		if(1*arrC[j]>1*min_value && 1*arrC[j]<1*max_value){
			try{
						objOption = document.createElement("option");
						objOption.value = arrC[j];
						objOption.appendChild(document.createTextNode(arr_text[j]));
						changeField.appendChild(objOption);
					}
					catch(e){}}
			}
	changeField.value=n_max;
} 
