      <textarea class="field_padding" name="Description" placeholder="Comments" style="width: 490px"></textarea><br/><br/>
      <tr>
		<td>Verification Code:
          <span class="feature-border">
            <div id="varifyNum" style="font-size: 14px; font-family: Arial,Helvetica,sans-serif; font-weight: bold;"></div>
          </span>
        </td>
        <td class="feature-border" align="left" valign="middle" width="66%">
          <input name="varifyNumHidden" id="varifyNumHidden" value="701469" type="hidden">                                          
          <input name="enterVerify" id="enterVerify" type="text" placeholder="Enter Verification Code Here">
        </td>
        <br/>
    </div>
    <div class="modal-footer">
      <button type="button" class="btn field_padding" data-dismiss="modal">Close</button>
