  <script>
    // Facebook
    var FB_APP_ID = '212500508799908';
    window.fbAsyncInit = function() {
      var FB = (window.FB || undefined);
      if (FB) {
        FB.init({
          appId      : FB_APP_ID
        , channelUrl : window.root_url+'/channel.html'
        , status     : true
        , cookie     : true
        , xfbml      : true
        });
      }
