function output_creative (id)
	{	if ( dfp_unit == 'MW_VOCAB' || dfp_unit == 'MW_SPELL' || dfp_unit == 'MW_NTT' || dfp_unit == 'MW_GAME-MENU' || dfp_unit == 'MW_GAMES' || dfp_unit == 'MW_TOF' )
			{	var index	= id.substr(id.length - 1, 1);
				var width	= index == 3 ? '160' : ( index == 1 || index == 5 ? '728' : '300' );
				var height	= index == 3 ? '600' : ( index == 1 || index == 5 ? '90' : '250' );
				var ord		= 2147483648*Math.random() + '.' + new Date().getTime();
				var tag = '<iframe id="ad-spot-' + index + '" src="/ad-tags/reload.php?id=' + id + '&unit=34444486/Merriam-Webster/' + dfp_unit + '&ord=' + ord + '" frameborder="0" marginheight="0" marginwidth="0" scrolling="no" style="margin: 0; padding: 0; width: ' + width + 'px; height: ' + height + 'px;"></iframe>';
				document.write(tag);
			}

		else
			{	document.write
					("<div id='" + id + "'>" + 
						"<scr" + "ipt type='text/javascript'>\r\n" + 
						"googletag.cmd.push(function() { googletag.display('" + id + "'); });\r\n" +
						"</sc" + "ript>" +
						"</div>");
			};
	};

function reloadCreatives (url)
	{	for ( var index = 1; index <= 5; index++ )
			{	var ad = document.getElementById('ad-spot-' + index);
				if ( ad )
					{	var src = ad.src.substring(0, ad.src.indexOf('&ord='));
						src += '&ord=' + 2147483648*Math.random() + '.' + new Date().getTime();
						ad.src = src;
					};
			};

	// Track the virtual page.
                if ( url != null )
                        {       url = typeof(url) != 'undefined' ? url : '' + document.location;
                                _gaq.push(['_trackPageview', url]);
                        };
	};

