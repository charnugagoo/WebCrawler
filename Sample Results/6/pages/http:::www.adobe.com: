                <script type="text/javascript">        	
			$(document).ready(function() {
				var newsItems = $(".NewsReelContent .NewsItem");
				var newsItemsLength = newsItems.length;
				var fadeDuration = 3000;
				var totalRounds = 6;
				
				var i = 1;
				var r = 1;			
				
				function fadeNews() {			
					if(r == totalRounds) return;
					$("#NewsItem"+i).fadeOut(fadeDuration,function() {
						
						if(i == newsItemsLength) { i = 0; r++ }
						
						$("#NewsItem"+(i+1)).fadeIn(fadeDuration);
						
						i++;
						
						fadeNews();
					});
				}
				
				fadeNews();	
			});		
        </script> 
                </div>
                <div class="LayoutRowBottomAll"></div>
