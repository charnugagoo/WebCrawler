    var jiveTitle = document.title;
    var pos = jiveTitle.indexOf("*");    
    if( (pos != -1) && (pos+1 != jiveTitle.length) ) {
        jiveTitle = jiveTitle.substr(pos+1,jiveTitle.length);
    }
