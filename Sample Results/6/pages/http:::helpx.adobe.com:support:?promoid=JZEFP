        var deepLinkMap = new Object();
        if(hash != null && hash != '') {
            $(document).ready(function() {
                var deepLink = deepLinkMap[hash];
                if(deepLink != undefined && deepLink != '') {
                    $('#' + deepLinkMap[hash] + ' a[name=' + deepLinkMap[hash] + ']').click();
                }
            });
