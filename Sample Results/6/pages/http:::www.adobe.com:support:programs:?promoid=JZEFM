    function createNs ( ns )
    {
        var o, a;           
        a = ns.split(".");          
        o = window[a[0]] = window[a[0]] || {};          
        for(i=1; i < a.length; i++)
        {
            o = o[a[i]] = o[a[i]] || {};    
        }
        return o;       
    }

