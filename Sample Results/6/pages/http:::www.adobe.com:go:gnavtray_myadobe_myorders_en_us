<script type="text/javascript">
    jQuery.createNs("Adobe.Account.Labels");
    Adobe.Account.Labels = {
        CurrencySymbol                  : 'US $',
        countryCodeName                 : 'United States',
        directDebitLabel                : '',
        common : {
            locale                      : "en_US",
            
            orderLabel                  : 'Order:',
            billingLabel                : 'Billing address:',
            billedToLabel               : 'Billed to:',
            bankTransferLabel           : '',
            convenienceStoreLabel       : '',
            shippingInfoLabel           : 'Shipping information:',
            shippingLabel               : 'Shipping',
            serialLabel                 : 'Serial:',
            serialErrorMessage          : 'Serial: Contact customer service',
            quantityLabel               : 'Quantity:',
            subtotalLabel               : 'Subtotal',
