document.observe("dom:loaded", function() {
	$$(".dyn-treenav").collect(function(el) {
		return new adobe.Tree(el, {
			style_uninitialized: "dyn-treenav",
			style_initialized: "treenav",
			select_button: ".tree-button"
		},
		{
			toggle_all: "Alt+click to show/hide all features"
		});
	});
});
</script>




