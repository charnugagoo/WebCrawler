<script type="text/javascript">
    $('.register_link').click(function(){
        $('.complete_layover').show();
        $('#register').show();                                               
    });
    
    var $login_show = 0;
    $('#login_link').click(function(){
        if($login_show == 0) {
            $('#login').show();
            $login_show = 1;
        } else {
            $('#login').hide();
            $login_show = 0;
        }                                              
    });

    $('.close_cross,#confirm_answer_no_img').click(function(){
        $('.complete_layover').hide();
        //$(this).parent().hide();    
        $('#confirm').hide();    
    });
</script>
    
