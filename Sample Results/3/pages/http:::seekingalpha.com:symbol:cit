<script>
  var page_url = 'http://seekingalpha.com/symbol/cit';
  var currTab = null;
  var page = 0

  SeekingAlpha.Initializer.onDOMLoad(function(){
    var pa = {slugs:"cit",subjects:5};
    var insertStockTalks = function(response) {
      if (response.total_pages > 0) $('more_stocktalk_link').style.display = 'block';
      var con = $('ajax_st_result');
      if (con){
        con.innerHTML = '';
        new Insertion.Top($('ajax_st_result'),'<div class="list" id="stocktalk_list">'+response.html+'</div>');
      }
    }
    var onFailed = function() {
      var con = $('ajax_st_result');
      if (con){
        con.innerHTML = '';
        new Insertion.Top($('ajax_st_result'),'<div class="list" id="stocktalk_list"></div>');
      }
      var stBlock = $('block_stocktalks');
      if (stBlock) stBlock.style.display = "none";
    }
    SA.Pages.Quote.init();
      create_ad('symbol_headline_dart',2, 't', true);
      $('auhtorpick_tabs').parentNode.className = "tab_quote_headlines";
      currTab = "tab_quote_headlines";
    Event.observe(window, 'scroll', SA.Pages.Quote.check_ad);
  });

  function create_ad(unit, tile, pos, notSticky){
    if ($(unit) && $(unit).innerHTML == ""){
      notSticky = (notSticky === undefined) ? false : true;
      NEW_AD_DELTA = 0;
      ad_src = 'http://ad.doubleclick.net/adj/sek.quotes/stocks-non-refresh;sz=300x250;x=x;tile=TILE;d=quotes;pos=POS;t=stocks-non-refresh;qp=true;t=financial;s=cit;'+dart_my_vocation_and_profiles()+'ord='+ord+'?';
      ad_src = ad_src.sub("TILE", tile).sub("POS", pos);
      SA.Utils.Ads.add({el:$(unit), HadCreated:false, size:'300x250', delta:NEW_AD_DELTA, info:ad_src, NotSticky: notSticky});
      SA.Utils.Ads.run();
    }
  }

  /* Navigation Functions */
  function add_selected_icon(row_id) {
    if ($(row_id)){
      $(row_id).className = 'selected';
    }
  }

  function trackNavLinkEvent() {
    try {
      var category = this.parentNode.parentNode.select('span.various_nav_title')[0].innerHTML;
      trackLink(this,'Quote Page Tracking', category, this.innerHTML);
    } catch(e) {}
  }

  $$('.various_nav a').each(function(link) {
    if (/^\/|seekingalpha\.com/i.test(link.href)) {
      try {
        var source = ('qp_nav_' + link.innerHTML).toLowerCase().replace(/&amp;/ig, '').replace(/\W+/g, '_');
        link.setAttribute('sasource', source);
      } catch(e) {}
    } else link.observe('click', trackNavLinkEvent);
  });
</script>
