<script type="text/javascript">
  function add_selected_icon(row_id) {
    $(row_id).className = 'selected';
  }

  var some = 0;
  sublinks = ["earnings","dividends","m-a","on-the-move"];
  sublinks.each(function(o){
    if (location.href.match(o)){
      add_selected_icon("currents_"+o);
      some++;
    }
  });
  if (some == 0) {add_selected_icon("currents_all");}

  function trackNavLinkEvent() {
    try {
      var category = this.parentNode.parentNode.select('span.various_nav_title')[0].innerHTML;
      trackLink(this,'Quote Page Tracking', category, this.innerHTML);
    } catch(e) {}
  }

  $$('.various_nav a').each(function(link) {
    if (/^\/|seekingalpha\.com/i.test(link.href)) {
      try {
        var source = ('qp_nav_' + link.innerHTML).toLowerCase().replace(/&amp;/ig, '').replace(/\W+/g, '_');
        link.setAttribute('sasource', source);
      } catch(e) {}
    } else link.observe('click', trackNavLinkEvent);
  });
</script>

