function closeSurvey() {
	location.href = Confirmit_MessOthers.original_url;
	if ($("iframe#helpSurvey") != null) {
		$("iframe#helpSurvey").dialog('close');
	}
	if ($("div#helpSurveyContainer") != null) {
		$("div#helpSurveyContainer").remove();
	}
}

function getURLParam(name) {
	name = name.replace(/[\[]/,"\\\[").replace(/[\]]/,"\\\]");
	var regexS = "[\\?&]" + name + "=([^&#]*)";
	var regex = new RegExp(regexS);
	var results = regex.exec(window.location.href);
	if (results == null) {
		return "";
	} else {
		return results[1];
	}
}

function gotoSurveySite(url) {
	closeSurvey();
	if (!(url == null || url == '')) {
		var pageURL = document.location.href;
		var match = new RegExp("[&\?]", "ig");
        var replaceURL = "";
        if (pageURL.length > 0) {
			replaceURL = pageURL.replace(match, "/");
		}

		var searchQ = '';
		if(searchQ =='' || null==searchQ ||searchQ =='null' ){
			searchQ = getURLParam("question_box");
		}

		if (!(searchQ == null || searchQ == '')) {
			window.open(url + '?parameter1=PROD_NEWS&parameter2=' + replaceURL + '&parameter3=' + searchQ);
		} else {
			window.open(url + '?parameter1=PROD_NEWS&parameter2=' + replaceURL);
		}
	}
}
