<script type="text/javascript">
    var currentCat;
    function megaHoverOver() {

        currentCat = $(".current");
        if (currentCat) currentCat.removeClass("current");
        $(this).find(".sub").stop().fadeTo('fast', 1).show();
        $(this).addClass("current");
        $(".subsel").stop().hide();
        
    }
    function megaHoverOut() {
        $(this).find(".sub").stop().hide();
        $(this).removeClass("current");
        $(".subsel").stop().fadeTo('fast', 1).show();
        if (currentCat) currentCat.addClass("current");
	}
	
	var config={sensitivity:2,interval:50,over:megaHoverOver,timeout:0,out:megaHoverOut}
	
	$(document).ready(function() {		
	   $("#searchTopNav ul li .sub").css({ 'opacity': '0' });
	   $("#searchTopNav ul li.category").hoverIntent(config);		
	});
</script>
