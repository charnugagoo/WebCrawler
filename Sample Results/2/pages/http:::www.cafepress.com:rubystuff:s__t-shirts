<script type="text/javascript">
    var thisIsStore = 1;

    $(document).ready(function() {
        var resultsAreaWidth = $('#resultsAreaWidth').width();
        if (resultsAreaWidth < 530) {
            var resultItems = $("#shopSearchResults .resultItem");
            resultItems.addClass("resultItemNarrow");
        }
        if (resultsAreaWidth > 635) {
            var resultItems = $("#shopSearchResults .resultItem");
            resultItems.addClass("resultItemWide");
        }
        if ($('#searchterm'))
            $('#searchterm').attr('value', "");
    });
</script>


