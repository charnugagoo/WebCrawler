<script type="text/javascript">
(function (w) {
    if(!w.gbCSM) {
    w.gbCSM = {
        markers: ['bb','cf','be','af'],

        getDataset: function() {
            return {
                conditions: 1,
                handler: function(marker,scope,widget) {
                    w.gbCSM.defaultHandler(marker,scope,widget);
                }
            };
        },

        defaultHandler: function(marker,scope,widget) {
            if(!marker) return;
            if(typeof uet == 'function') {
                if(!scope) { uet(marker); }
                else if(widget) {
                    uet(marker,scope,{wb:1});
                    if(marker == 'be' && typeof uex == 'function') {
                        uex('ld',scope, {wb:1});
                    }
                } else{
                    uet(marker,scope);
                }
            }
        },

        initialize: function(scope) {
            if(!scope || this[scope]) {
                return;
            }

            this[scope] = {};
            for(var i in this.markers) {
                var marker = this.markers[i];
                this[scope][marker] = this.getDataset();
            }
            return this[scope];
        },
        signalMarker: function(scope,marker,ajax) {
            if(!this[scope] ||
                !this[scope][marker] || this[scope][marker].conditions <= 0) {
                return;
            }

            if(--this[scope][marker].conditions == 0) {
                if(typeof this[scope][marker].handler == 'function') {
                    if(ajax) {
                        this[scope][marker].handler(marker,scope);
                    } else {
                        scope == 'goldbox' ? this[scope][marker].handler(marker) :
                                    this[scope][marker].handler(marker,scope,true);
                    }
                }
            }
        }
    };
    }

    var gbmarkerobj = w.gbCSM.initialize('goldbox');

    gbmarkerobj.cf.defaulthandler = gbmarkerobj.cf.handler;
    gbmarkerobj.cf.handler = function(marker,scope,widget) {
        this.defaulthandler(marker,scope,widget);

        gbCSM.signalMarker('goldbox','af');
    };

    gbmarkerobj.cf.conditions = 2;

    gbmarkerobj.af.conditions = 2;

    var dotdmarkerobj = w.gbCSM.initialize('dotd');

    dotdmarkerobj.cf.defaulthandler = dotdmarkerobj.cf.handler;
    dotdmarkerobj.be.conditions = 2;
    dotdmarkerobj.cf.handler = function(marker,scope,widget) {
        this.defaulthandler(marker,scope,widget);

        gbCSM.signalMarker('dotd','be');
    };

    dotdmarkerobj.be.defaulthandler = dotdmarkerobj.be.handler;
    dotdmarkerobj.be.handler = function(marker,scope,widget) {
        this.defaulthandler(marker,scope,widget);

        gbCSM.signalMarker('goldbox','cf');
    };

    var gbldmarkerobj = w.gbCSM.initialize('gbld');

    gbldmarkerobj.cf.defaulthandler = gbldmarkerobj.cf.handler;

    gbldmarkerobj.be.conditions = 2;
    gbldmarkerobj.cf.conditions = 2;
    gbldmarkerobj.cf.handler = function(marker,scope,widget) {
        this.defaulthandler(marker,scope,widget);

        gbCSM.signalMarker('gbld','be');
    };

    gbldmarkerobj.be.defaulthandler = gbldmarkerobj.be.handler;
    gbldmarkerobj.be.handler = function(marker,scope,widget) {
        this.defaulthandler(marker,scope,widget);

        gbCSM.signalMarker('goldbox','cf');
    };

})(window);
</script>


