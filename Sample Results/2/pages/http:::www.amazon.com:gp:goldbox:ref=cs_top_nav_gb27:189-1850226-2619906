  </div>
<~ } ~>

  

</script>

<script id="price_block135" type="text/template">
        <div id="dealPriceBlock" class="priceblock">
    <~
	var listPrice = null;
	var dealPrice = null;
        var isVariation = false;

        if(data.listPrice && data.listPrice.isRange
            && data.listPrice.max && data.listPrice.min) {
            listPrice = data.listPrice.min.formattedPrice + ' - ' + data.listPrice.max.formattedPrice;
            isVariation = true;
        } else if(data.listPriceFormatted) {
            listPrice = data.listPriceFormatted;
        } else if(data.ourPrice && data.ourPrice.isRange
            && data.ourPrice.max && data.ourPrice.min) {
            listPrice = data.ourPrice.min.formattedPrice +
                        ' - ' + data.ourPrice.max.formattedPrice;
            isVariation = true;
        } else if(data.ourPriceFormatted) {
            listPrice = data.ourPriceFormatted;
        }

	if(data.dealPrice && data.dealPrice.isRange
        && data.dealPrice.max && data.dealPrice.min) {
            var priceSeparator = ' - ';
            if(gbResources.getCustomerData('realm') == 'CN') {
                priceSeparator = '-';
            }

            dealPrice = data.dealPrice.min.formattedPrice + priceSeparator + data.dealPrice.max.formattedPrice;
	    isVariation = true;
        } else if(data.dealPriceFormatted) {
		    dealPrice = data.dealPriceFormatted;
        }
    ~>
	<span id="dealDealPrice"><b>$( dealPrice )</b></span>
        <~ if(!isVariation && data.discountPercentage) { ~>
 		<span id="dealPercentOff"><~ print(gbResources.getString('csld-percent_off', {discountPercentage: data.discountPercentage})) ~></span>
        <~ } ~>
    <~ if(data.marketingMessage) { ~>
            <span id="dealMarketingMessage" class="marketingmessage"><b>$( data.marketingMessage)</b></span>
    <~ } ~>
    </div>
</script>

