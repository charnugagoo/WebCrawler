<script type="text/template" id="time_remaining135">
<~
    var timerSpanID = data.dealID + '_time_remaining';
    var timeString = '';
    var alertTxt = '';
    if (data.timeRemainingTimer !== undefined &&
       data.timeRemainingTimer !== null) {
        data.timeRemainingTimer.viewElement = data.viewElement;
        data.timeRemainingTimer.spanID = timerSpanID;
        timeString = data.timeRemainingTimer.timeString;
    }
    
    if (data.msToEnd !== undefined &&
        data.msToEnd !== null && 
        data.msToEnd < 3*60*60*1000) {
        alertTxt = 'ldtimetxtalert';
    }
~>
    <div id="dealTimeRemaining" class="ldtimeleft $(alertTxt)">
        <span id="$(timerSpanID)">$(timeString)</span>
    </div>
</script>

<script type="text/template" id="progress_bar135">
<~
    var pcntClaimedFormatted = data.percentClaimed + '%';

    var statebarClass    = 'ld-one';
    var progbarClass     = 'progbar';
    var progbartickClass = 'progbartick';

    if (data.dealState == data.stateToTemplateMap.Expired ||
        data.dealState == data.stateToTemplateMap.SoldOut) {
        statebarClass    = 'ld-four';
        progbarClass     = 'progbarmissed';
        progbartickClass = 'progbartickmissed';

    } else if (data.percentClaimed !== undefined && data.percentClaimed !== null &&
      data.percentClaimed > 80 && data.percentClaimed < 100) {
        progbarClass = 'progbaralert';
    }

~>
<div class="ldtimercont">
    <div class="statebar $(statebarClass)"></div>
    <div class="$(progbarClass)" style="width:$(pcntClaimedFormatted)"></div>
    <div class="$(progbartickClass)" style="width:$(pcntClaimedFormatted)"></div>
</div>
</script>
<script id="title135" type="text/template">
    <~
    var refTaggedURL = data.detailPageLink;
    if(data.refTags && data.refTags.title && data.detailPageLink) {
        if(data.dealType == Deal.dealTypes.LOCAL) {
            refTaggedURL = Deal.generateRefTaggedExternalRedirectURL(data.detailPageLink, data.dealID, data.refTags.title, data.amabotParams);
        } else {
            refTaggedURL = Deal.ref_tag_appender(data.detailPageLink,data.dealID,data.refTags.title, data.amabotParams);
        }
    }
    data.dealTitleTruncated = Deal.truncate_description (data.dealTitle, 20);

    var escapedTitle = Deal.escapeHtml(data.dealTitle);

    var titleClass = "title";
    ~>
    <div id="dealTitle" class='$(titleClass)'>
      <a id="dealTitleLink" href='$( refTaggedURL )' title="$( escapedTitle )">
    <~ if(gbResources.getCustomerData('realm') != 'CN' &&
        gbResources.getCustomerData('realm') != 'JP') {
        ~>
            $(data.dealTitle)
        <~
        } else {
        ~>
            $(data.dealTitleTruncated)
        <~
        }
        ~>
      </a>
    </div>
</script>

