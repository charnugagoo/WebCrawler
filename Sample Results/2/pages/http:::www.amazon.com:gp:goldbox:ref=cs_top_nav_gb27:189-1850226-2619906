    var upcoming = true;

    if (data.dealState == 'upcoming' && data.dealTeaser !== undefined &&
        data.dealTeaser !== null && data.dealTeaser != '') {
        imageURL = gbResources.getImage ('hero_upcoming');

    } else if(data.refTags && data.refTags.image && data.detailPageLink) {
        upcoming = false;
        if(data.dealType == Deal.dealTypes.LOCAL) {
            refTaggedURL = Deal.generateRefTaggedExternalRedirectURL(data.detailPageLink,data.dealID,data.refTags.image,data.amabotParams);
        } else {
            refTaggedURL = Deal.ref_tag_appender(data.detailPageLink,data.dealID,data.refTags.image,data.amabotParams);
        }
    }
    
    if(imageURL !== undefined && imageURL !== null) {
        imageURL = Deal.resizeImage(imageURL, 135);
    }
    ~>

