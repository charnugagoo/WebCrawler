        ContextWindow.opener = this;
        ContextWindow.focus();
}
function RefreshOriginalWindow(url) {
        if ((window.opener == null) || (window.opener.closed))
        {
                var OriginalWindow = window.open(url);
                OriginalWindow.opener = this;
        }
        else{
