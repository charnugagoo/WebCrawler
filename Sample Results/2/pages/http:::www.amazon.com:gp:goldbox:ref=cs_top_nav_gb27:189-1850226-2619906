<script type="text/javascript">
    amznJQ.onReady ('jQuery', function() {

        if(!window.Deal) {
            window.Deal = {};
        }

        if(!window.Deal.configManager) {
            Deal.configManager = 







new DealConfigManager({
    controllerConfig: {
   "versionInfo" : "",
