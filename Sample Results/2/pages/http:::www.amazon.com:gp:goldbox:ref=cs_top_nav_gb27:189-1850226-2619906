<script type="text/template" id="state135">
<~
    var timerSpanID = data.dealID + '_state_timer';
    var pendingAtc  = false;

    var inCart      = false;
    var inCartCount = 0;

    var inWaitlist      = false;
    var inWaitlistCount = 0;

    var isVariation = false;

    var checkoutUrl = '/gp/cart/view.html';

    if (data.variationData !== undefined &&
        data.variationData !== null) {
        isVariation = data.variationData.isVariation;
    }

    if (data.stateDisplayData[Deal.inCartStr] !== undefined &&
        data.stateDisplayData[Deal.inCartStr] !== null &&
        data.stateDisplayData[Deal.inCartStr].length > 0) {
        inCart = true;
        inCartCount = data.stateDisplayData[Deal.inCartStr].length;
    }

    if (data.stateDisplayData[Deal.pendingAtcStr] !== undefined &&
        data.stateDisplayData[Deal.pendingAtcStr] !== null &&
        data.stateDisplayData[Deal.pendingAtcStr].length > 0) {
        pendingAtc = true;
    }

    if (data.stateDisplayData[Deal.waitInLineStr] !== undefined &&
        data.stateDisplayData[Deal.waitInLineStr] !== null &&
        data.stateDisplayData[Deal.waitInLineStr].length > 0) {
        inWaitlist = true;
        inWaitlistCount = data.stateDisplayData[Deal.waitInLineStr].length;
    }

    var importantState = 'Available';

    var message = '';

    if (pendingAtc) {
        importantState = Deal.PendingAddToCart;

        data.stateDisplayData[Deal.pendingAtcStr].timerObject.viewElement =
          data.viewElement;
        data.stateDisplayData[Deal.pendingAtcStr].timerObject.spanID =
          timerSpanID;

        message = gbResources.getString ('gbd_deal_atc_time_part1') + ' ' +
                  ' <span class="gbcheckouttime" id="' + timerSpanID + '">' +
                  data.stateDisplayData[Deal.pendingAtcStr].timerObject.timeString
                  + '</span> ';
        message += gbResources.getString ('gbd_deal_atc_time_part2') ?
                    gbResources.getString ('gbd_deal_atc_time_part2') : '';

    } else if (inCart && !inWaitlist) {
        importantState = Deal.inCartStr;

        message = '<b>';
        if (isVariation) {
            if (inCartCount > 1 &&
                inCartCount == data.stateDisplayData.totalAsins) {
                message += gbResources.getString('gbd_all_options_in_cart');
            } else if (inCartCount == 1) {
                message += gbResources.getString('gbd_1_option_in_cart');
            } else if (inCartCount > 1) {
                message += gbResources.getString('gbd_num_options_in_cart', {num: inCartCount});
            }

        } else {
            message += gbResources.getString('gbd_deal_in_cart');
        }
        message += '</b>';

        message += '<br/>';

        message += '<a href="' + checkoutUrl + '">' +
                    gbResources.getString ('csld-check_out')
                    + '</a>';

        message += ' ' + gbResources.getString ('csld-in') + ' ';

        message +=  ' <span class="gbcheckouttime" id="' + timerSpanID + '">' +
                    data.stateDisplayData[Deal.inCartStr].timerObject.timeString
                    + '</span> ';

        data.stateDisplayData[Deal.inCartStr].timerObject.viewElement =
          data.viewElement;
        data.stateDisplayData[Deal.inCartStr].timerObject.spanID = timerSpanID;

        if (inCartCount > 1) {
            message +=
              gbResources.getString ('gbd_check_out_within_time_part2_plural') ?
                gbResources.getString ('gbd_check_out_within_time_part2_plural')
                           : '';
        } else {
            message +=
             gbResources.getString ('gbd_check_out_within_time_part2_single') ?
                gbResources.getString ('gbd_check_out_within_time_part2_single')
                : '';
        }
    } else if (inWaitlist && !inCart) {

        importantState = Deal.waitInLineStr;

        message = '<b>';

        if (inWaitlistCount == 1) {
            if (isVariation) {
                message += gbResources.getString('gbd_1_option_waitlisted');
            } else {
                message += gbResources.getString('gbd_you_are_waitlisted');
            }
        } else if (inWaitlistCount > 1 &&
                   inWaitlistCount == data.stateDisplayData.totalAsins) {
            message += gbResources.getString('gbd_all_are_waitlisted');
        } else if (inWaitlistCount > 1) {
            message += gbResources.getString('gbd_num_options_in_waitlist', {num: inWaitlistCount});
        }

        message += '</b>';

    } else if (inWaitlist && inCart) {

        data.stateDisplayData[Deal.inCartStr].timerObject.viewElement =
          data.viewElement;
        data.stateDisplayData[Deal.inCartStr].timerObject.spanID = timerSpanID;

        importantState = Deal.waitInLineStr + ' & ' + Deal.inCartStr;

        var inCartStr = '<b>' + inCartCount + ' ' +
                        gbResources.getString('gbd_in_cart') + '</b> ';

        inCartStr += '<a href="' + checkoutUrl + '">' +
                     gbResources.getString ('csld-check_out')
                     + '</a>';
        inCartStr += ' ' + gbResources.getString ('csld-in') + ' ';

        inCartStr += ' <span class="gbcheckouttime" id="' + timerSpanID + '">' +
                     data.stateDisplayData[Deal.inCartStr].timerObject.timeString
                     + '</span>';
        inCartStr += '</b>';

        var inWaitlistStr = '<b>';
        inWaitlistStr += inWaitlistCount + ' ' +
                            gbResources.getString('gbd_on_waitlist');
        inWaitlistStr += '</b>';

        message = inCartStr + '<br/>' + inWaitlistStr;

    }


~>

<div class="dealtypecont">
    $(message)
</div>

</script>

