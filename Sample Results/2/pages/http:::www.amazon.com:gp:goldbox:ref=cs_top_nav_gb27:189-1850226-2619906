<script id="reviews135" type="text/template">
<~  if (data.reviews !== undefined 
        && data.reviews.stars !== undefined) {

    var hasReviews = true;

    if (data.reviews === null || data.reviews.stars === null ||
        parseFloat (data.reviews.stars) < 1) {
        hasReviews = false;
    }

    var rating = data.reviews.stars;

    var rounded = Math.round(rating*2)/2;
    var stars = 'stars_'+Math.floor(rounded)+'_'+(rounded%1*10);

    var starsImg = gbResources.getImage (stars);


    var refTaggedURL = data.detailPageLink;
    if(data.refTags && data.refTags.reviews && data.reviews.reviewsURL) {
        refTaggedURL = Deal.ref_tag_appender(data.reviews.reviewsURL,data.dealID,data.refTags.reviews, data.amabotParams);
    }
~>
    <div id="dealRatingAndCount" class="ratingcontainer">
    <~ if(hasReviews) { ~>
	<a id="dealRating" href= $( refTaggedURL )>
		<img src = $( starsImg ) />
	</a>
	<a id="dealReviewsCount" class="gbox-small" href= $( refTaggedURL )>
	    ($( data.reviews.count ))
	</a>
    <~ } else {
        print('<a href="' + refTaggedURL + '">');
        print(gbResources.getString('csld_add_a_review'));
        print('</a>');
    } ~>
    </div>
<~ } ~> 
</script>
<script id="please_wait135" type="text/template">
