<script id="title135" type="text/template">
    <~
    var refTaggedURL = data.detailPageLink;
    if(data.refTags && data.refTags.title && data.detailPageLink) {
        if(data.dealType == Deal.dealTypes.LOCAL) {
            refTaggedURL = Deal.generateRefTaggedExternalRedirectURL(data.detailPageLink, data.dealID, data.refTags.title, data.amabotParams);
        } else {
            refTaggedURL = Deal.ref_tag_appender(data.detailPageLink,data.dealID,data.refTags.title, data.amabotParams);
        }
