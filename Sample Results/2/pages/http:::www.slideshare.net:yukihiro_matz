        function getErrorTag(error){
          var tag = "Javascript errors";
          var errors = {'GA_google' : 'Ad errors',
                        'exception' : 'Browser errors',
                        'Script error' : 'Browser errors',
                        'translate' : 'Google translate errors'};
          for( i in errors ){
            if (error.match(i)) tag = errors[i];
          }
          return tag;
        }
        try{
          window.onerror = function(error,url,line){
            var errStr = "error : "+error+"\n url : "+url+"\n line no : "+line;
            window._gaq = _gaq || [];
            _gaq.push(["_trackEvent", getErrorTag(error), errStr , window.location.href ]);
            if(window.console)
              console.log(errStr);
            return true;
          };
