(function(r) {
  var set = function(name, value, days) {
    var date = new Date();
    date.setTime(date.getTime()+days*86400000);
    document.cookie = name+"="+value+"; expires="+date.toGMTString()+"; path=/; domain=.shopify.com";    
  };
  var match1 = new RegExp("[\\?&](source|r|ref)=([^&#]*)", "gi").exec(r);
  if(match1) {
    var days = (match1[2] === 'cj') ? 45 : 7;
    set("source", match1[2], days);
  }
  var match2 = new RegExp("[\\?&]PID=([^&#]*)", "gi").exec(r);
  if(match2) {    
    set("cj_pid", match2[1], 45);
  }
})(window.location.href);
</script>


