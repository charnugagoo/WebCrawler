<script type="text/javascript">
function replaceSearchTextCommaToPipe() {
var searchText = document.getElementById('isbn-search').value;
if (document.getElementById('isbn-search').value == 'Enter a title, author, or ISBN(s)') searchText = ''; 
document.getElementById('isbn-search').value = searchText.replace(/,/g,"|");
return true;
}
</script>


