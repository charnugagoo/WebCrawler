 <script language="javascript">
   (function($,$$) {
     function isPaymentPending() {
       if(window.cookie && window.cookie('proNotice') === null) {
         $.ajax({
           url: '/business/premium/is_payment_pending_or_churned_out',
           type: 'POST',
           dataType: 'json',
           success: function(reply){
             if (reply.status === true) {
               if(!reply.churn || (reply.churn && window.cookie('churnNotice') === null)) {
                 var element_array = [$('header'), $('div.headerWrapper'), $('.navbar')];
                 $.each(element_array, function() {
                   if(this.length > 0) {
                     this.append(reply.msg);
                   }
                 });
               }
               $('#j-close_pro_notice').click(function(e){
                 e.preventDefault();
                 $('#j-pro_notice').slideUp('slow', function(){
                  if(reply.churn) {
                    cookie('churnNotice', true, {expires: 365 * 24 * 60 * 60 * 10}); // expires after 10 years
                    } else {
                    cookie('proNotice', true, {expires: 0.006944}); // 10 minutes
                  }
                  $('#j-pro_notice').remove();
                });
              });
              $('#j-pro_notice').slideDown('slow');
            }
          }
        });
      }
    }
    function notification() {
      $.ajax( {
        url: "/newsfeed/get_newsfeed_count",
        type: 'GET',
        async: true,
        dataType: 'json',
        success: function ( data ){
          if (data.count > 0) {
            $('.j-intro-newsfeedcount').html('check ' +data.count);
            $('.j-icon_NoNotification, .j-icon_Notification').toggleClass('hide');
            $('.j-icon_Notification span').html(data.count);
            if (typeof Tinycon !== 'undefined'){
              Tinycon.setBubble(data.count);
            }
          }
        },
        error: function (jqXHR){
        }
      });
    }
      
      $(".j-icon_NoNotification").click(function(e){
        recordGAMenuEvent('Newsfeed', 'NoNotification_clicked');
      });
      
      $(".j-icon_Notification").click(function(e){
        recordGAMenuEvent('Newsfeed', 'Notification_clicked');
      });
      
      $(".j-help").click(function(e){
        recordGAMenuEvent('Dropdown_Menu_Top_navigation', 'help_topnav_clicked');
      });
      
      $(".j-newsfeed").click(function(e){
        recordGAMenuEvent('Dropdown_Menu_Top_navigation', 'newsfeed_topnav_clicked');
      });
      
      $(".j-public-profile").click(function(e){
        recordGAMenuEvent('Dropdown_Menu_Top_navigation', 'public_profile_topnav_clicked');
      });
      
      $(".j-uploads").click(function(e){
        recordGAMenuEvent('Dropdown_Menu_Top_navigation', 'my_uploads_topnav_clicked');
      });
      
      $(".j-zipcasts").click(function(e){
        recordGAMenuEvent('Dropdown_Menu_Top_navigation', 'my_zipcasts_topnav_clicked');
      });
      
      $(".j-edit-profile").click(function(e){
        recordGAMenuEvent('Dropdown_Menu_Top_navigation', 'edit_profile_topnav_clicked');
      });
      
      $(".j-analytics").click(function(e){
        recordGAMenuEvent('Dropdown_Menu_Top_navigation', 'analytics_topnav_clicked');
      });

      $(".j-pro-dashboard").click(function(e){
        recordGAMenuEvent('Dropdown_Menu_Top_navigation', 'pro_dashboard_topnav_clicked');
      });
      
      $(".j-logout").click(function(e){
        recordGAMenuEvent('Dropdown_Menu_Top_navigation', 'logout_topnav_clicked');
      });
      
      // Record GA event for the menu item 
      function recordGAMenuEvent(menuItem, event) {
        _gaq.push(['_trackEvent', menuItem, event]);
      }
      
      $(window).load(function(){
        if($$.user.loggedin) {
          notification();
          isPaymentPending();
        }
      });
    })(jQuery,slideshare_object);

    </script>
    
