        var lazyLoadURL = '/gp/product-ads/lazyLoad/handler/click-within-handler.html';
        var cw = {};
          amznJQ.available('search-js-general', function() {
            SPUtils.afterEvent('searchajax', function() {           
              var bl = unescape(jQuery.pageContext.browseLadder);

              var cwurl = location.href;
              var regexRh =  /([?&=])(rh=)([^&]*)/;
              var regexSort = /([?&=])(sort=)([-\w]+)/; 
              var regexMe = /([?&=])(me=)([^&]*)/; 
              var regexScn = /([?&=])(scn=)([-\w]+)/; 
              var hashindex = cwurl.indexOf("#"); 
              var rhstring;
              var sortstring;
              if (hashindex == -1) 
              {
                rhstring = regexRh.exec(cwurl);
                sortstring = regexSort.exec(cwurl);
              } else {   
                hashstring = cwurl.substring(hashindex+1);
                rhstring = regexRh.exec(hashstring); 
                sortstring = regexSort.exec(hashstring);
              }
            if (rhstring != null && rhstring.length != 0) 
              {
                var rh = unescape(rhstring)
                       , params = rh.split(",")
                       , len = params.length;
      
                var pat = /(\b[n|k|i]\b)/; 
                if (len > 0 ) {
     				      for (var x = 0; x < len; x++) {
                   var entry, rhKey;
             	     entry = params[x].split(":"); 
          		     if (entry.length == 2) {
                     rhKey = entry[0];                 
                     if(!pat.test(rhKey)) { 
                       jQuery('#click_withinLazyLoad_tower').html("<div></div>");
                       return;
                     }
   	               } 
      	          }
         	      }
            }
            if(regexMe.test(unescape(cwurl))) { 
              jQuery('#click_withinLazyLoad_tower').html("<div></div>");
              return;
            }
              
            if(sortstring != null && sortstring.length != 0) {
              var srt = unescape(sortstring)
                       , params = srt.split(",")
                       , len = params.length;
                     
              if (len > 0 ) {
               for (var x = 0; x < len; x++) { 
                var entry, sortVal;
                entry = params[x].split("=");
                if (entry.length == 2) {
                   sortVal = entry[1];
                   if(sortVal != "relevancerank" && sortVal != "") {
                    jQuery('#click_withinLazyLoad_tower').html("<div></div>");
                    return;
                   }
                }  
               }
              }   
      
            }

            if((typeof(window.pa_bl_tower) != "undefined" && window.pa_bl_tower != bl) || (typeof(window.pa_kw_tower) != "undefined" && window.pa_kw_tower != iss.keyword())) {
            window.pa_asins_tower = undefined;
            } else {
              if(typeof(asinList) != "undefined") {
                if(asinList == 0) {
                  jQuery('#click_withinLazyLoad_tower').html("<div></div>");
                  return;
                } else if(typeof(window.pa_asins_tower) != "undefined") { 
                   window.pa_asins_tower = window.pa_asins_tower + asinList;
                   var temp = window.pa_asins_tower.split(",");
                   if(temp.length > 40 ) {
                     var extra = temp.length - 40; 
                     for (var i = 0; i < extra; i++) {
                        temp.shift();
                     }
                   } 
                   window.pa_asins_tower = temp.join();
                  } else {  
                    window.pa_asins_tower = asinList; 
                  }
               }
             }
              

              window.pa_bl_tower = bl; 
              window.pa_kw_tower = iss.keyword();
              window.pa_sa_tower = SearchPageAccess.searchAlias();
   
                       
              if (bl == null || bl.length == 0)
              {
                jQuery('#click_withinLazyLoad_tower').html("<div></div>");
                return; /* return if browse-ladder is null */
              }
              var suggestedBN = [];

              if (bl == 'i:aps') {
                var urlattr = jQuery("#refinements > ul > li > a:first").attr("href"); 
                if(urlattr) { 
                 var m = urlattr.match(regexScn);
                 if (m && m.length>=4) {
                    suggestedBN.push(m[3]);
                 }
                }                
              }

              var bnlist = bl.split(',');
              var bn = bnlist[0].replace(":","");          
              var kw = iss.keyword();
              var sa = SearchPageAccess.searchAlias();
              if (
                  !(browseNodeMap.hasOwnProperty(bn) || bl == 'i:aps') ||
                  /* We are not under the allowed list of browse-nodes */
                  !kw)                         /* no search terms => no ads to fetch */
              {
                  jQuery('#click_withinLazyLoad_tower').html("<div></div>"); 
                  return;
              }
              cw.sa = sa;
              cw.kw = kw;
              cw.bl = bl;

              jQuery.ajax({
                  
                  url: lazyLoadURL + "?searchTerms="+escape(cw.kw)+"&searchAlias="+escape(cw.sa)+
                                     "&isDebug=0&widgetName="+escape(widgetName)+
                                     "&pageType=Search&subPageType=dynamic-superset-3"+
                                     "&browseLadder="+escape(cw.bl)+
                                     "&placement=tower"+"&seenasins="+escape(window.pa_asins_tower)+
                                     "&suggestedBN="+escape(suggestedBN),
                  type: "GET",
                  cache : false,
                  success : function(html) { 
                   jQuery('#click_withinLazyLoad_tower').html(html);    
                  },
                  error : function(html) { 
                   jQuery('#click_withinLazyLoad_tower').html("<div></div>");    
                  }
              });
          });
         });
       });
}



  </script>




















 
 
 


