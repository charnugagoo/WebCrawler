r.className=r.className.replace(new RegExp('s9m'+String.fromCharCode(92)+'d+','g'),'s9m'+n);
return {potentialCols:pn,cols:n}
}
}
amznJQ.onReady('s9Multipack', function() {
  jQuery('#ns_0ACD11A13645421D96BA_244_r2ItemRow').s9Multipack({ minItems: 3, maxItems: 5, minItemWidth: 145, seeded: false, seedHeaderBottomPadding: 6});
