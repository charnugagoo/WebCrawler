jQuery(document).ready(function() {
	jQuery(".panelswitch").click(switchPanel);
});
	
function switchPanel(event) {
    //var target = jQuery(event.target);
    jQuery(".messagetype-login").height(jQuery(".messagetype-login").height());
    //jQuery("#front-panel").css("position","absolute"); /*due to firefox bug */
    jQuery("#front-panel").fadeOut(200);
    jQuery("#signin-panel").fadeIn(200);
    return false;
}
</script>

