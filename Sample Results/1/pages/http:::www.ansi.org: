	function GetResults()
	{
		var	f =	document.forms[0];

		if(f.elements['searchstring'].value == '')
		{
			alert('Please enter one or more search words.');
			return false;
		}
		else
		{
			f.action = 'http://publicaa.ansi.org/sites/apdl/_layouts/1033/searchresults.aspx';
			f.submit();
		}
	}
	</script>
