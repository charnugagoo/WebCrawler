if(i==5) elem.firstChild.data="Order Page of UNIVERSAL REPORT";
if(i==6) elem.firstChild.data="UNIVERSAL SOFTWARE Products";
if(i==7) elem.firstChild.data="Affiliate Program";
if(i==8) elem.firstChild.data="Contact Information and Form";
im = 'url(new_img_template/qt'+i+'.jpg)';
elem2.style.backgroundImage=im;

}
