function relocate_flash(){
 var t=document.body.clientWidth;
  var IsIE=navigator.userAgent.toLowerCase().indexOf('msie');
 if (t>970){
   t = (t-970)/2;
   document.getElementById("flash_box").style.left=t-31;
 }else{ document.getElementById("flash_box").style.left=-21;}
   if (IsIE>-1)
    {document.getElementById("flash_box").style.marginTop='1px';}
  else
   {document.getElementById("flash_box").style.marginTop='-6px';}
   relocate_flash_start();
 //  window.location.reload();
}
