}
function flag_in(lang){
if (lang=='eng') document.getElementById("ver").innerHTML='English version of the overview';
if (lang=='fr') document.getElementById("ver").innerHTML="Version française de l'introduction";
if (lang=='sp') document.getElementById("ver").innerHTML='Versión española de la introducción';
if (lang=='de') document.getElementById("ver").innerHTML='Deutsch Version der Einleitung';
}
function flag_out(){
  document.getElementById("ver").innerHTML='&nbsp;';
}
function set_intro(lang){
if (lang=='eng') document.getElementById("intro").innerHTML=document.getElementById("engver").innerHTML;
if (lang=='fr') document.getElementById("intro").innerHTML=document.getElementById("frver").innerHTML;
if (lang=='sp') document.getElementById("intro").innerHTML=document.getElementById("spver").innerHTML;
if (lang=='de') document.getElementById("intro").innerHTML=document.getElementById("dever").innerHTML;
}
function anim_intro(){
 document.getElementById("intro").innerHTML="<img border=0 src=welcome.jpg>";
 setTimeout("hide_opaque_w()",4000);
}
function hide_opaque_w(){
 tt = document.getElementById('intro').style.opacity;
 tt = parseFloat(tt);;
 if(tt>0.025){
  dec_opacity_w();setTimeout("hide_opaque_w()",25);
 }else{
   set_intro('eng');
   make_opaque_w();
 }
}
function dec_opacity_w(){
 tt = document.getElementById('intro').style.opacity;
 tt = parseFloat(tt);
 if(tt>=0.025) tt=tt-0.025; else tt=0;
  document.getElementById('intro').style.opacity=tt;
  try{document.getElementById('intro').filters.alpha.opacity=100*tt;}catch(err){}
 
}
function inc_opacity_w(){
 var tt,i,j;
 tt = document.getElementById('intro').style.opacity;
 tt = parseFloat(tt);
 if(tt+0.01<=1) tt=tt+0.01; else tt=1;
  document.getElementById('intro').style.opacity=tt;
  try{document.getElementById('intro').filters.alpha.opacity=100*tt;}catch(err){}
 
}
function make_opaque_w(){
 tt = document.getElementById('intro').style.opacity;
 tt = parseFloat(tt);
 if(tt<1){inc_opacity_w();setTimeout("make_opaque_w()",25);}
}


function recommend(){
window.open('home_recommend.htm','_blank','top=0,left=0,width=600,height=500,resizable,scrollbars');
}

function inc_opacity(){
 var tt,i,j;
 tt = document.getElementById('ur_screen').style.opacity;
 tt = parseFloat(tt);
 if(tt+0.01<=1) tt=tt+0.01; else tt=1;
  document.getElementById('ur_screen').style.opacity=tt;
  try{document.getElementById('ur_screen').filters.alpha.opacity=100*tt;}catch(err){}
 
}


function dec_opacity(){
 tt = document.getElementById('ur_screen').style.opacity;
 tt = parseFloat(tt);
 if(tt>=0.025) tt=tt-0.025; else tt=0;
  document.getElementById('ur_screen').style.opacity=tt;
  try{document.getElementById('ur_screen').filters.alpha.opacity=100*tt;}catch(err){}
 
}
function make_opaque(){
 tt = document.getElementById('ur_screen').style.opacity;
 tt = parseFloat(tt);
 if(tt<1){inc_opacity();setTimeout("make_opaque()",25);}else{setTimeout("hide_opaque()",4000); }
}

function hide_opaque(){
 tt = document.getElementById('ur_screen').style.opacity;
 tt = parseFloat(tt);
 if(tt>0){
  dec_opacity();setTimeout("hide_opaque()",25);
 }else{
   screens();
 }
}


function screens(){
 ScreenId +=1; if(ScreenId==9) ScreenId=0;
 document.getElementById("ur_screen").src="new_img_template/new_screen_"+ScreenId+".jpg";
 make_opaque();
}

