function validate(email) {
    var reg = /^([A-Za-z0-9_\-\.]{2,})+\@([A-Za-z0-9_\-\.]{2,})+\.([A-Za-z]{2,4})$/;
    if(reg.test(email) == false)
       return true;
    else
         return false;
}
function navshowtext(){
	if(document.getElementById('navnewsletteremailID').value == 'Email address'){
