	    function rewireScrollTo() {
	        if (typeof (WebForm_OnSubmit) == 'function') {
	            oldScrollTo = window.scrollTo;
	            window.scrollTo = function (x, y) {
	                if (x == 0 && y == 0)
	                    return true;
	                else
	                    return oldScrollTo(x, y);
	            }
	        }
	    }
