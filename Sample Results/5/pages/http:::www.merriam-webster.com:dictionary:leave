					<script type="text/javascript">
	// The function shows the facebook login.
	var FB_callback = null;
	function showFBLogin (callback)
		{	// document.getElementById('facebook-login-win').style.display = 'table';
			try
				{	FB.curr_user.callback = FB_callback = typeof(callback) !== 'undefined' ? callback : null;			
				}

			catch (exp)
				{	
				};
			
			try
				{	FB.login(null,{perms:''});
				}

			catch (exp)
				{
				};

			return false;
		};

	// Override FB status changes.
	function onFBStatusChange (response)
		{	if ( typeof(updateMySavedWordsDisplay) !== 'undefined' )
				{	updateMySavedWordsDisplay();		
				};
			if ( FB.curr_user.callback && FB.curr_user.callback !== null )
				{	FB.curr_user.callback();
					FB.curr_user.callback = null;
				};
		};



	// This function checks if a user is currently logged into the My Words feature.
	function my_words_logged_in (callback)
		{	if ( getCookie('my_saved_logged_in', 0) == 1 )
				{	return true;
				}

			else
				{	showFBLogin(callback);
					return false;
				};
		};
</script>
