			<script type="text/javascript">
			function honorMemory(i) {
				
					var n = i.charAt(i.length-1);
					if (n==1) {
					document.getElementById('other_designation_type2').checked=true;
					document.getElementById('COMMENTS2').value="";
					}else if (n==2) {
					document.getElementById('other_designation_type3').checked=true;
					document.getElementById('COMMENTS1').value="";
					}
					
			}
			</script>				
			<div class="header-giftInHonor">
