<script type="text/javascript">
    function submitForm()
    {
        document.getElementById('AMOUNT_GFT_1').value = document.getElementById('gift_1').value;
        document.getElementById('AMOUNT_GFT_2').value = document.getElementById('gift_2').value;
		
		if (document.givingForm.recurringSubscriptionInfo_numberOfPayments.value!='') { //validate the form for a recurring payment scripts/cart.js
		var ok = checkRecurring();
			
		}else { //validate the form for one-time gift
        var ok = checkFunds(); 
		}
		if (ok == 1)
			  {
				  document.getElementById('givingForm').submit();
			  }
    }
			
			
			function makeDate(myDate){
				//extract year, month, date numeric values back out of Date()
				var d=myDate.getDate();
					d= addZero(d);
				var m=myDate.getMonth()+1; //month value 0-11
					m=addZero(m);
				var yr=myDate.getFullYear();
				
				//put back together in format YYYYMMDD
				var strDate = yr.toString()+m+d;
				
				return strDate;
			}
			function addZero(n){
				if (n.toString().length==1){
					n="0"+n;	
					}
					return n;
			}
			
			
			function formatMoney(m,id) {
				var numM= new Number(m);
				
				if (numM < 5) { 
				
				numM = 5;
				document.getElementById('gift_1_display').innerHTML = "To make a gift of less than $5.00, please contact the NYU Office of Gift Administration on 1-800-698-4144.";
				}
				
				if (isNaN(numM)==false){
					
				document.getElementById(id).value=numM.toFixed(2); 	
				}else {
					
					var numNew = m.split(",");
					var numRepl="";
					for (x in numNew) {
						numRepl=numRepl+numNew[x];
						}
				document.getElementById(id).value=Number(numRepl).toFixed(2);
				}
				
			}
			
			//Recurring Gift Functions
			function giftSwitch(g) {
			
				if (g=="oneTimeGiftContent") { //show one time gift; reset all values in recurring gift
					
					//reset all radio buttons
					for (i=0;i<document.givingForm.recurringGiftAmount.length;i++) {
					document.givingForm.recurringGiftAmount[i].checked = '';
					}
					//reset other value
					clearOther();
					
					document.givingForm.recurringSubscriptionInfo_numberOfPayments.value=""; //NOTE: a value in this field triggers the middleware to ignore gift_1 amt and process as recurring
					document.givingForm.recurringSubscriptionInfo_amount.value="";
					document.givingForm.item_0_unitPrice.value="";
					document.givingForm.recurringSubscriptionInfo_frequency.value="";
					document.givingForm.recurringSubscriptionInfo_startDate.value="";	
					
					//show add'l gift button
					document.getElementById('btn-addGift').style.display="block";
						
					//show one time gift section and hide recurring section
					document.getElementById('oneTimeGiftContent').style.display="block";
					document.getElementById('RecurringGiftContent').style.display="none";
				
					
				}
				if (g=="RecurringGiftContent") { //show recurring gift; reset all values in one time gift
					//unset any values for gift if recurring is checked
					document.getElementById('gift_1').value="";
					document.getElementById('AMOUNT_GFT_1').value="";
					//unset any values for second gift if recurring gift is checked
					document.givingForm.gift_2.value="";
					document.givingForm.AMOUNT_GFT_2.value="";
					document.givingForm.fund2.value="";
					//hide second gift input fields if shown
					document.getElementById('btn-addGift').style.display="none";
					document.getElementById('header-additionalGift').style.display="none";
					document.getElementById('additionalGiftContent').style.display="none";
						
					//show recurring section and hide one time gift section
					document.getElementById('oneTimeGiftContent').style.display="none";
					document.getElementById('RecurringGiftContent').style.display="block";
					
					//set recurringSubscriptionInfo_numberOfPayments.value NOTE: a value in this field triggers the middleware to ignore gift_1 amt and process as recurring
					calcSubscription();
				}
				
			}
			
			function calcSubscription() {
				var f = new Number(document.givingForm.gift_frequency.value);
				var d = new Number(document.givingForm.num_payments.value);
								
				var numPayments= f*d;
				
				
				document.givingForm.recurringSubscriptionInfo_numberOfPayments.value=(numPayments-1);//subtract one for first payment
				
				setFrequency();
				
				
			}
			function setFrequency() {
				
				var f = document.givingForm.gift_frequency.value;
				
				switch (f) {
				case "52":
				document.givingForm.recurringSubscriptionInfo_frequency.value="weekly";
				break; 	
				case "26":
				document.givingForm.recurringSubscriptionInfo_frequency.value="bi-weekly";
				break;
				case "24":
				document.givingForm.recurringSubscriptionInfo_frequency.value="semi-monthly";
				break;
				case "13":
				document.givingForm.recurringSubscriptionInfo_frequency.value="quad-weekly";
				break;	
				case "12":
				document.givingForm.recurringSubscriptionInfo_frequency.value="monthly";
				break;
				case "4":
				document.givingForm.recurringSubscriptionInfo_frequency.value="quarterly";
				break;
				case "2":
				document.givingForm.recurringSubscriptionInfo_frequency.value="semi-annually";
				break;
				case "1":
				document.givingForm.recurringSubscriptionInfo_frequency.value="annually";
				break;
				default:
				break;
				}
				
			}
			
			//Set Recurring Gift - Fill in
			
			function selectOther() {
			
			if (document.givingForm.recurringGiftAmount[6].checked != 'checked') {
				
			document.givingForm.recurringGiftAmount[6].checked = 'checked';
			document.getElementById('recurringGiftOtherAmt').focus();
			}
			}
			function clearOther() {
				document.getElementById('recurringGiftOtherAmt').value='';	
			}
			
			function setRGift(g) {
					if (g=='other') { //if Other amount is selected
					
							document.getElementById('item_0_unitPrice').value=document.getElementById('recurringGiftOtherAmt').value;
							document.getElementById('recurringSubscriptionInfo_amount').value=document.getElementById('recurringGiftOtherAmt').value;
					} else { //recurring gift is set by radio button value
							document.getElementById('item_0_unitPrice').value=g+'.00';
							document.getElementById('recurringSubscriptionInfo_amount').value=g+'.00';
						
					}
					setStartDate();
			}
			
			// Recurring payment frequency choices
			
			function checkYear() {//when a user selects annual recurring payment; remove 1 year from duration choices
			if (document.givingForm.gift_frequency.value=="1") { 
					
					document.givingForm.num_payments.options.length=0;
					document.givingForm.num_payments.options[0]=new Option("2 years", "2");
					document.givingForm.num_payments.options[1]=new Option("3 years", "3");
					
					
					} else {
					document.givingForm.num_payments.options.length=0;
					document.givingForm.num_payments.options[0]=new Option("1 year", "1");
					document.givingForm.num_payments.options[1]=new Option("2 years", "2");
					document.givingForm.num_payments.options[2]=new Option("3 years", "3");
						
					}
			
			}
			
			function setStartDate() { //first recurring payment date is one month from today's date by default; when user selects different option, start date changes
				var today = new Date();
				var Y;
				var m;
				var d;
				
				
				if (document.givingForm.gift_frequency.value==12) {
					
					today.setMonth(today.getMonth()+1);// one month from today (monthly payments)
					Y=today.getFullYear();
					m=addZero(today.getMonth()+1); // month in format 0-11 (add one, append zero to front)
					d=addZero(today.getDate());
					
					document.givingForm.recurringSubscriptionInfo_startDate.value=''+Y+m+d;
				
				}
				if (document.givingForm.gift_frequency.value==4) {
					
					today.setMonth(today.getMonth()+3);// three months from today (quarterly payments)
					Y=today.getFullYear();
					m=addZero(today.getMonth()+1); // month in format 0-11 (add one, append zero to front)
					d=addZero(today.getDate());
					
					document.givingForm.recurringSubscriptionInfo_startDate.value=''+Y+m+d;
					
				}
				if (document.givingForm.gift_frequency.value==1) {
					today.setMonth(today.getMonth()+12);// 12 months from today (yearly payments)
					Y=today.getFullYear();
					m=addZero(today.getMonth()+1); // month in format 0-11 (add one, append zero to front)
					d=addZero(today.getDate());
					
					document.givingForm.recurringSubscriptionInfo_startDate.value=''+Y+m+d;
					
				}
			
			}
			
			
			</script>

  
