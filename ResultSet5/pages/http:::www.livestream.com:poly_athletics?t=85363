var mbd = {
        uagent : navigator.userAgent.toLowerCase(),
        currentUrl : window.location.toString(),
        redirect : function(deviceUrl){
                window.location.replace(deviceUrl);
        },
        hide_mobile_promo: document.cookie.indexOf('hide_mobile_promo') != -1,
        detect : function(){
                if((mbd.uagent.search("iphone") > -1 || mbd.uagent.search("ipod") > -1) && !mbd.hide_mobile_promo)
                  mbd.redirect("http://www.livestream.com/mobile_promo.html");
                else {
                  return false;
}
        }
}
mbd.detect();
