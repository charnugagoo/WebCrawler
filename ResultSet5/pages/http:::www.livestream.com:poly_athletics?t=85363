  <div id="chat-livestream" class="tab highlight"></div>
  <script type="text/javascript">
	var so = new SWFObject("http://cdn.livestream.com/chat/LivestreamChat.swf", "livestreamChat" , "100%", "100%", ((navigator.platform.toLowerCase().indexOf("linux") != -1) ? "9.0.31.0" : "9.0.45.0"), "#ffffff");
	so.addVariable("channel", "poly_athletics");
	so.addVariable("showBorder", "false");	
	so.addParam("wmode","transparent");
	so.addParam("allowScriptAccess", "always");
