<script type="text/JavaScript">
	function displayFormRow(formRow)
	{   
		document.getElementById(formRow).style.display = '';
	}
	
	function hideFormRow(formRow)
	{
		document.getElementById(formRow).style.display = 'none';
	}
	
	function changeSearchBy()
	{
	searchType = document.getElementById('searchBook').value;
		hideFormRow('STUDENT');
		hideFormRow('COURSE');
		hideFormRow('TITLE');
		hideFormRow('AUTHOR');
		hideFormRow('ISBN');
		displayFormRow(searchType);
	}

</script>


