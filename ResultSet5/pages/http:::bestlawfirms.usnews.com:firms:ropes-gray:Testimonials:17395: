    <script type="text/javascript">
        function doPanelToggle(sender, eventArgs) {
            var item = eventArgs.get_item();
            if (item.get_expanded()) {
                item.collapse();
                eventArgs.set_cancel(true);
            }
        }

        function doPracticeAreaJump(ddl) {
            var val = ddl.options[ddl.selectedIndex].value
            if (val != 0) {
                var a = String.format("practiceArea{0}", val)
                window.location.hash = a
            }
        }

        function pageLoad() {
            $('#divPracticeAreasPreviewContent').truncateToHeight(200, '...')
        }
    </script>
     
    
