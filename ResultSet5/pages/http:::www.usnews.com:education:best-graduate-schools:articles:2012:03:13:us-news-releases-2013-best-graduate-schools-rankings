<script type="text/javascript">
  if(window.jQuery === undefined) {
    USN.is_Prototype = true;
    google.load("jquery", "1.4.4");
    google.load("jqueryui", "1.8.6");
  }
</script>
<script type="text/javascript">
  if(USN.is_Prototype !== undefined) {
    jQuery.noConflict();
  }
</script>
<script type="text/javascript">
  jQuery(document).ready(function() {
    //tracking pixels
   var ord = USN.Ad.ord;
    jQuery('.ets-quiz .subwidget-last').append('<img src="http://ad.doubleclick.net/ad/N4864.USNWR/B7260725.33;sz=1x1;ord=' + ord + '?" width="1" height="1" /><img src="http://ad.doubleclick.net/activity;src=1565908;type=quest280;cat=qod-s413;ord=' + ord + '?" width="1" height="1" />');

    jQuery('.ets-quiz').each(function() {
      var $quiz_widget = jQuery(this);
      $quiz_widget.find('.response-options p').click(function() {
        jQuery(this).siblings().removeClass('selected');
        jQuery(this).addClass('selected');
      });
      $quiz_widget.find('.submit').click(function() {
        var is_correct = $quiz_widget.find('p.selected').hasClass('correct');
        if (is_correct) {
          $quiz_widget.find('.answer h5').addClass('right-answer');
        }
        $quiz_widget.find('.response-options').hide();
        $quiz_widget.find('.answer').show();
      });
      $quiz_widget.find('.no-js').hide();
      $quiz_widget.find('.prompt, .ets-quiz .response-options').show();
      $quiz_widget.find('.response-options p:first').trigger('click');
    });
  });
</script>
