<script type="text/javascript">
	$(function () {
        function flog (f) {
            iclog.logClick(function () { f.submit(); },
                $(f).attr(wsNs.constants.coidAttribute), $(f).attr(wsNs.constants.copAttribute));
        }
        
		$('#topSearchForm').validate({
		   rules: { q: { required: true }, ql: { requiredIfVisible: true } },
		   messages: { q: { required: '' }, ql: { required: '' } },
           submitHandler: flog
		});

        $('#bottomSearchForm').validate({
		   rules: { q: { required: true }, ql: { requiredIfVisible: true } },
		   messages: { q: { required: '' }, ql: { required: '' } },
           submitHandler: flog
		});
        $('#topWPSearchForm').validate({
		   rules: { qf: { required: true }, qn: { required: true }, qc: { required: true }, qs: { required: true }},
		   messages: { qf: { required: '' }, qn: { required: '' }, qc: { required: '' }, qs: { required: '' } },
           submitHandler: flog
		});
        
        $('#stockSearchForm').validate({
		   rules: { q: { required: true } , q: { notEqual: 'Enter company symbol' } },
		   messages: { q: { required: ''}, q: { notEqual: ''}},
           submitHandler: flog
		});
        $('#weatherSearchForm').validate({
		   rules: { q: { required: true }, q: { notEqual: 'Enter US City State or Zip' }},
		   messages: { q: { required: '' }, q: { notEqual: ''} },
           submitHandler: flog
		});

        $('#navigationalMarketingForm').validate({
		   rules: { q: { required: true } } ,
		   messages: { q: { required: '' } },
           submitHandler: flog
		});

		$('#advancedSearchForm').validate({ submitHandler: flog });

	});
   
</script>
