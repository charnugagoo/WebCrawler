	<script type="text/javascript" language="JavaScript">
		 // <![CDATA[
				subNavList = new Playlist('subNav');
				subNavMan = new UrlPlaylistManager(subNavList);
				subNavMan.pane = new Pane("_layout");
				subNavMan.activeClass="navOver";
				subNavMan.inactiveClass="navOff";
				subNavMan.pane.scrape('/sectionSubNav?id=7238216&section=&objId=4380645&urlBase=');
				subNavATimer = null;
				subNavDTimer = null;
				subNavActive=false;

				
									subNavMan.addItem("section","/sectionSubNav?id=7238216&section=&objId=4380645&urlBase=");
									
									subNavMan.addItem("sectionus","/sectionSubNav?id=16360130&section=us&objId=4380645&urlBase=");
									
									subNavMan.addItem("sectioninternational","/sectionSubNav?id=16360143&section=international&objId=4380645&urlBase=");
									
									subNavMan.addItem("sectionpolitics","/sectionSubNav?id=7238258&section=politics&objId=4380645&urlBase=");
									
									subNavMan.addItem("sectionvideo","/sectionSubNav?id=9163734&section=video&objId=4380645&urlBase=");
									
									subNavMan.addItem("sectionblotter","/sectionSubNav?id=7238440&section=blotter&objId=4380645&urlBase=");
									
									subNavMan.addItem("sectionhealth","/sectionSubNav?id=7238407&section=health&objId=4380645&urlBase=");
									
									subNavMan.addItem("sectionentertainment","/sectionSubNav?id=7238287&section=entertainment&objId=4380645&urlBase=");
									
									subNavMan.addItem("sectionbusiness","/sectionSubNav?id=7238288&section=business&objId=4380645&urlBase=");
									
									subNavMan.addItem("sectiontechnology","/sectionSubNav?id=7238235&section=technology&objId=4380645&urlBase=");
									
									subNavMan.addItem("sectiontravel","/sectionSubNav?id=7238239&section=travel&objId=4380645&urlBase=");
									
			
			subNavShow = function(){
				window.setTimeout(function(){
					$('#siteSubNav').fadeIn(100, function(){});
					subNavMan.pane.resize();
				}, 100);
			 }
			
			subNavHide = function(){
				$('#siteSubNav').hide();
			}
			
			/* subnav timers */
			subNavReset = function(){
				subNavHide();
				subNavMan.playlist.gotoItem("section");
				subNavDTimer=null;
				subNavActive = false;
			};

			subNavReset();
			subNavMouseOut = function(){
				if(subNavATimer != null){
					window.clearTimeout(subNavATimer);
				}
				if(subNavDTimer==null){
					subNavDTimer = window.setTimeout(subNavReset,500);
				}
			};
			subNavMouseOver=function(){
				if(subNavDTimer!=null){
					window.clearTimeout(subNavDTimer);
					subNavDTimer=null;
				}
			};
			subNavGoto = function(obj){
				subNavMouseOver();
				var o = obj;
				var time = subNavActive ? 100:200;
				if(subNavATimer != null){
					window.clearTimeout(subNavATimer);
					subNavATimer = null;
				}
				subNavATimer = window.setTimeout(function(){
					subNavActive = true;
					subNavList.gotoItem(o.id);
					o=null;
					subNavShow();
				},time);
				subNavList.prepare(o.id);
				
			}
			subNavMan.pane.addMouseOut(subNavMouseOut);
			subNavMan.pane.addMouseOver(subNavMouseOver);
						
			
			// ]]>
		 </script>

