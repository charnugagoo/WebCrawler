function trackBzDock(){
	if(bzDTry < bzDTryLimit){
		if($('#AdBarAdHeader').length > 0 || $('.yontoolayerwidget').length > 0){
			bzDFlag = true;
			clearTimeout(bzDInt);
		}else{
			bzDInt = setTimeout("trackBzDock()", 2000);
		}
		bzDTry++;
	}
	if(bzDFlag){
		var s = s_gi(s_account);
		s.trackExternalLinks=false;
		s.linkTrackVars='events';
		s.linkTrackEvents=s.events='event1';
		s_omni.tl(true, 'o', 'Buzzdock Alert');
		
	}
};
