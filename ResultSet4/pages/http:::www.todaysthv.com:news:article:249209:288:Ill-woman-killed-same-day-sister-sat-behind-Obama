 if((document.title==undefined)||(document.title=="")){DT=NO_TITLE_VAR;}else{DT=document.title;}
 myPageName=DT+" | "+QS; 
 if(DT.indexOf(" | ") != -1){DT=document.title;DTA=DT.split(" | ");DT=DTA[0];}
