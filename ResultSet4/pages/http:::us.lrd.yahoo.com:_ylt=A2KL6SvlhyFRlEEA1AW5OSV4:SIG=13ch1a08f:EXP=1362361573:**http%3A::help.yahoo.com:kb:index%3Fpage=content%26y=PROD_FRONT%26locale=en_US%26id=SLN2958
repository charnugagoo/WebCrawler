<script type="text/javascript">
	$(document).ready(function(){
		// custom rating javascript for content page
		var ratingStars = $('div#FIVE_STAR').text().trim().split(/\s{2,100}/);
		for (var i = 0; i < $('ul.rating li a').size(); i++) {
		  $($('ul.rating li a').get(i)).attr('title', ratingStars[i]);
		}

		$('div#helpfeedbackexp li').remove();
		var ratingQuestions = $('div#QUESTION').text().trim().split(/\s{2,100}/);
		for (var i = 0; i < ratingQuestions.length; i++) {
		  var questionRadio = '<input name="yhelpFeedbackSurvey" class="radio" id="yhelpFeedbackSurvey' + (i+1) + '" value="' + (i+1) + '" type="radio">';
		  $('div#helpfeedbackexp ul').append('<li>' + questionRadio + ' ' + ratingQuestions[i] + '</li>');
		}

		var ratingComment = $('div#COMMENT').text().trim();
		$('div#feedbackexp2 textarea[name=ratingComments]').attr('prompt_text', ratingComment);
		$('div#feedbackexp2 div.form-prompt-text').text(ratingComment);


		if ($('title') != null && $('title').html() != null) {
			// remove encoded <span></span> tags from title
			document.title = removeSpans($('title').html()) ;
		}
	});
      </script>
