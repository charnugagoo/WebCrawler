<script type="text/javascript">
	function adjustArcticleFontSize(scale)
	{
		if(scale == 'fss') {
			document.getElementById('article-texsize').style.fontSize = '100%';
			document.getElementById('fss').className = "fss fsactive";
			document.getElementById('fsm').className = "fsm";
			document.getElementById('fsl').className = "fsl";
			captureChangingTextSize('small');
		}
		if(scale == 'fsm') {
			document.getElementById('article-texsize').style.fontSize = '113%';
			document.getElementById('fss').className = "fss";
			document.getElementById('fsm').className = "fsm fsactive";
			document.getElementById('fsl').className = "fsl";
			captureChangingTextSize('medium');
		}
		if(scale == 'fsl') {
			document.getElementById('article-texsize').style.fontSize = '138%';
			document.getElementById('fss').className = "fss";
			document.getElementById('fsm').className = "fsm";
			document.getElementById('fsl').className = "fsl fsactive";
			captureChangingTextSize('large');
		}
	}

	function sendFeedback()
	{
		// rating
		var rating = 0;
		rating = ($('ul.rating').hasClass('onestar-active')) ? 1 : rating;
		rating = ($('ul.rating').hasClass('twostar-active')) ? 2 : rating;
		rating = ($('ul.rating').hasClass('threestar-active')) ? 3 : rating;
		rating = ($('ul.rating').hasClass('fourstar-active')) ? 4 : rating;
		rating = ($('ul.rating').hasClass('fivestar-active')) ? 5 : rating;
		$($('form#ratingForm div#FIVE_STAR input')[rating-1]).attr('checked', true);

		if (rating > 0) {
			// bad rating question
			if (rating == 1) {
			var questionValue = $('input[name=yhelpFeedbackSurvey]:checked').val();
				$($('form#ratingForm div#QUESTION input')[questionValue-1]).attr('checked', true);
			}

			// comment
			$('form#ratingForm div#COMMENT textarea').val($('div#feedbackexp2 textarea').val());
		}

                if ($('form#ratingForm div#COMMENT textarea') === "The information you provide will be used to make this information more helpful in the future. We will not respond directly to your comments.") {
                        $('form#ratingForm div#COMMENT textarea').val("");
                }

		$('form#ratingForm').submit();
	}
</script>
