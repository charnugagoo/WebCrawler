<script type="text/javascript">
   //replace conditional-zip template with colonial raptor 
$(window).load(function() {
  //parse zipcode string from inspcookie and assign it to zipcode 
  var zip = getCookie("inspcookie");
 
  var cookies = zip.split("&");
  var name = "";
  var value = "";
  var myZip = "";
  for (i=0; i<cookies.length; i++)
  {
    name=cookies[i].substr(0,cookies[i].indexOf("="));
    value=cookies[i].substr(cookies[i].indexOf("=")+1);
   
    if (name == 'zip') {
      
      myZip = value;
    } 
  }
  //console.log(myZip);
  //if zip cookie is present, use colonial raptor to get city and state.  
  //update zip
  //update location text
  //update client side panels (local news, weather, reviews, events)  
   if (myZip) { 
Infospace.ColonialRaptor.location(myZip,
           { force_sync: 1 },
         function(data, status, xhr) {
           if (status == 'success') {
             setCookie("zip",data.zip,null);
             $('#usrLocation').html(data.city + ', ' + data.state);
             $(document).trigger("cr.location.zip", data.zip);
             //if (setRegion){setRegion(data.state);};
              } else {
                //console.log('fail');
           }  
     });     
     //if zip cookie is not present, use colonial raptor city, state and zip of current ip location
     //update the zip cookie
     //update the location text
     //update client side panels (local news, weather, reviews, events)
   } else {
     //console.log('zip is empty string')
      Infospace.ColonialRaptor.location_by_ip(null, null,
        function(data, status, xhr) {
          if(status == 'success') {
            setCookie("zip",data.zip,null); 
                $('#usrLocation').html(data.city + ', ' + data.state);
                $(document).trigger("cr.location.zip", data.zip);
            
             }
       });
    }

});
</script>

