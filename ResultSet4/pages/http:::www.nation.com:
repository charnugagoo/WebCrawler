<script type="text/html" id="eventHomeSectionTemplate">

[% for (var i=0; i<thesecategories.categories.length; i++) { %]
  [% var thiscategory = thesecategories.categories[i]; %]
    [% if (i == 0) { %]

        

  <h2 class="categoryHeadingLink inline-block"><a href="/p/events/source/events_ef">Events</a></h2>		
             <a href="http://eventful.com/" target="_blank">
               <img class="poweredByEventful" src="http://caprica.s3.amazonaws.com/production/versions/158/assets/poweredbyeventful.png" alt="" />
             </a>


         [% for (var j=0; j<3; j++) { %]
           [% if (!(article = thiscategory.articles[j])) { break; } %]
             <article class="eventHomeArticle">
                <div class="articleTeaser">
                  <p class="eventTitleLink clearfix"> 
                    <a href="[%= article.url %]" target="_blank">
                       [%= _.words(article.title).slice(0,7).join(' ') %]
                    </a> 
                  </p>
                  <time class="eventDate"
                    datetime="[%= new Date(article.start_time).toString() %]">
                  
                        [%=  moment(article.start_time).format('dddd, MMMM Do YYYY, h:mm a') %]
                  
                   
                  </time>
               [% if (article.image_url) { %]
                  <div class="eventImage" [% if (article.image_url.indexOf('html') !=-1) { %] style="display:none;" [% } %]>     
                    <div style="background-image:url('[%= article.image_url %]');" class="eventImage">
                    </div>
                  </div>
               [% } %]
                <p class="introCopy">
                  [%= _.stripTags(_.words(article.description).slice(0,article.image_url ? 10 : 20).join(' ')) %]
                </p>
              </div>
       	    </article>
         [% } %]

                   <p class="moreNewsLink clearfix"><a href="/p/events/source/events_ef">More Events &#9654;</a></p>

  [% } %]
[% } %]

</script>

<script type="text/javascript">
$(document).on('cr.location.zip', function(e, zip) {
    try { 
     
    var event_section = $('.eventHomeSection');

    Infospace.ColonialRaptor.source('events_ef',
       { zip: zip, force_sync: 1 },
       function(data, status, xhr) {

         if ( status == 'success') {
            event_section[0].innerHTML = doT.template($('#eventHomeSectionTemplate')[0].innerHTML)(null, data);
         } else {
           //alert('flop');
         }
           event_section.removeClass('loader');
     });
  }
  catch (e) {
    alert(e.message);
  }
    
});
  </script>
  

  
  
  
 
 

   
 
     
    
