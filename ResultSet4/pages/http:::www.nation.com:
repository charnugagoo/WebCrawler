 <script type="text/html" id="reviewHomeSectionTemplate">
      <h2 class="categoryHeadingLink inline-block">
        <a href="/p/reviews/source/reviews_yp">Reviews<span class="greenLine"></span></a>
      </h2>
      <a href="http://www.yelp.com" target="_blank">
        <img src="http://caprica.s3.amazonaws.com/production/versions/158/assets/poweredbyyelp.png" alt=""/>
      </a>
          
         [% for (var i=0; i<3; i++) { %]
           [% if (!(article = thiscategory.articles[i])) { break; } %]  
              <article class="reviewHomeArticle">
              <div class="articleTeaser">
                [% if (article.photo_url) { %]
                <div class="articleImage">     
                  <div style="background-image:url('[%= article.photo_url %]');" class="reviewImage">
                  </div>
                </div>
                [% } %]
                 <p class="largeHeadlineLink" > 
                    <a href="[%= article.url %]" target="_blank">
                            [%= article.name %]
                    </a> 
                 </p>
                <div class="rating_panel">
                  <span class="review_rating">
                    <img src="[%= article.rating_image_url %]" alt="[%= article.avg_rating %]"/>
                  </span>
                  <span class="review_count introCopy">
                    [%= article.review_count %] Reviews
                  </span>                  
                </div>
                 <p class="introCopy">
                   [% if (article.photo_url) { %]
                   [%= _.words(article.description).slice(0, 10).join(" ") %]
                   [% } else { %]
                   [%= article.description %]
                   [% } %]
                     
                 </p>
                

              </div>
            </article>
         [% } %]
                   
            
        <p class="moreNewsLink clearfix"><a href="/p/reviews/source/reviews_yp">More Reviews &#9654;</a></p>
  </script>
  

  
  <script type="text/javascript">
$(document).on('cr.location.zip', function(e, zip) {
      try {
    var _review_section = $('section.reviewHomeSection');
         Infospace.ColonialRaptor.category('reviews_yp', 'restaurants',
             { zip: zip, force_sync: 1 },
           function(data, status, xhr) {
             if (status != 'success' || !(data.articles instanceof Array)) {
               _review_section.append('<span>No content</span>');
               _review_section.addClass('splat');
             } else {
 
               _review_section[0].innerHTML = doT.template($('#reviewHomeSectionTemplate')[0].innerHTML)(data);

             }
             _review_section.removeClass('loader');
     });
      }
  catch (e) {
    alert(e.message);
  }
    });
  </script>

           
           
       
