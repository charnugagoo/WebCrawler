<script type="text/html" id="weatherPanelTemplate">
 
   <div id="phone-panel-weather-header">
    <h2 class="categoryHeading">Weather <a href="http://www.accuweather.com" target="_blank">
        <img src="http://caprica.s3.amazonaws.com/production/versions/158/assets/accuweather-logo-100rgba.png"/>
        </a></h2>
           
    [% var current = thiscategory.current; %]
  <span id="weaLocation" class="todaysDate"></span>
    
    
  <span id="weaDate" class="todaysDate"> February 18, 2013</span>
    
    
   
   
  <section class="curr_weather">
         <div class="current_temperature"> 
          <span class="temperature">[%= current.temp %]</span>
          <span class="degrees">         
            [%= current.temp_units %]
            </span>
          <img class="weather_icon" src="[%= current.weather_icon_small %]" /> 
    	 </div>
           <div class="current_weather">
             <span class="weatherConditions">[%= current.weather %]</span><br/>
             <span class="weatherConditions">Feels like: [%= current.real_feel_temp %]</span><br/>
            <span class="weatherConditions">
              Wind: [%= current.wind_direction %] [%= current.wind_speed %] [%= current.wind_speed_units %]
            </span>
          </div>
    </section>
    <div class="weather_forecast clearfix">
       <div class="forecast_header">
      [% var forecast = thiscategory.forecast; %]
      [% for ( i = 0; i < 5; i++ ) { %]
      [% if (i < 4) { %]
        [% if (i != 0) { %]
        <section style="border-left:1px solid #CED0CF;">
          [% } else { %]
        <section>
        [% } %]
            <header>
              <h3>[%= forecast[i].day_name.toString().slice(0,3) %]</h1>
              <h5 style="">[%= new Date(forecast[i].date).toString().slice(4, 10) %]</h3>
            </header>
            <div class="forecast_details">
              <img src="[%= forecast[i].weather_icon %]"/> 
              <span> 
                <span class="bold">[%= forecast[i].high_temp %]</span> / [%= forecast[i].low_temp %] 
              </span>
            </div>
          </section>
       [% } %]
       [% } %]
       </div>
    </div>
                  
  </div>                                                                                                 
  
</script>

<script type="text/javascript">
 
$(document).on('cr.location.zip', function(e, zip) {
    try {
      
    // get the div that is going to store the weather info
    var phone_panel = $('#weatherPanel');
    // call colonial raptor to get weather
    Infospace.ColonialRaptor.source('wea_ac'
           , { zip: zip, force_sync: 1 }
           , function(data, status, xhr) {
      if ( status == 'success') {
        phone_panel[0].innerHTML = doT.template($('#weatherPanelTemplate')[0].innerHTML)(data);
      }
      else {
        //alert('flop');
      }
        
    });
    
  }
  catch (e) {
    alert(e.message);
  }
    
    
  });
  
</script>
  
  
  
  
  

