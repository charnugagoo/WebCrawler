<script type="text/javascript">     
 $('#locEdit').click(
    function(){
     // updateLocation();
	 locationProxy();
 });
  
  function closeLocationEditPanel() {
    $("#locEditPanel").hide(500, "swing");
    }
  
   function locationProxy() {
    address = $("#locEditText").val();
     if( address == null || address.length == 0) {
      return;
    }
     
    gcGoogle(address);
    //update weather
    //updateWeather(null, address, '', 1, true);
  }
	
  function updateWeather(zip, city, state, skip_cookie, reload_page) {
    if( zip != null) {
      Infospace.ColonialRaptor.location(zip,
          { force_sync: 1, skip_cookie: skip_cookie },
          function(data, status, xhr) {
          if(status == 'success') {
            // we can avoid reloading weather module if we are re-loading the entire page
            if( reload_page ) { 
                setCookie("zip",zip,null);
                window.location.reload();                
            } else {
            }};
          });
     } else {
     Infospace.ColonialRaptor.location_by_city(city, state,
          { force_sync: 1, skip_cookie: skip_cookie },
          function(data, status, xhr) {
          if(status == 'success') {            
            if( reload_page ) { 
                setCookie("zip",data.zip,null);
                window.location.reload();                
            }};
          });
     }
  
  }
 
  function gcGoogle(address) {
    var gc = new google.maps.Geocoder();
    gc.geocode( { 'address': address, 'region': '021' }, function(results, status) {
        if (status == google.maps.GeocoderStatus.OK) {
          if (results.length == 1) {
            var addr = getAddr(results[0].address_components);
            if (addr.city && addr.state && addr.zipCode) {
              updateLocation(addr.city + ',' + addr.state);
              handleDefaultLocation(addr.zipCode);
            } else {
              
              var pos = { coords: {
                
                  latitude: results[0].geometry.location.lat(),
                  longitude: results[0].geometry.location.lng()
                }};
              //console.log("Google reverse geocode: " + pos);
                getLocationFromGoogle(pos);
            }
          } else if (results.length > 1) {
            var addrs = getAddresses(results, false);
            addrs = _.uniq(addrs, false, function(a) {
              return a.city + ',' + a.state;
            });
            if (addrs.length > 1) {
              showAddressDialog(addrs);
            } else if (addrs.length == 1) {
              getLocationFromGoogle(posFromAddr(addrs[0]));
              
            }
            else {
              //console.log("No results");  
              //locationError();
            }
            //console.log(addrs);
      
          } else {
            //console.log("No results");
          }
        } else {
          //console.log("Failed with " + status);
          locationError();
        }
      });
  }
  
  function posFromAddr(addr) {
    return { coords: {
      latitude: addr.latitude,
      longitude: addr.longitude
    }};
  }
  
  function showAddressDialog(addresses) {
    var list = $('<ul></ul>');
    for (var i=0; i<addresses.length; i++) {
      var addr = addresses[i];
      var pos = posFromAddr(addr);
      var entry = $('<li><a href="#">' + addr.city + ', ' + addr.state + '</a></li>');
      entry.click({pos: pos}, function(e) {
        hideAddressDialog();
        getLocationFromGoogle(e.data.pos);
      });
      list.append(entry);
    }
    
    $('#location-prompt').html(list);
    $('#location-prompt').show();
  }
  
  function hideAddressDialog() {
    $('#location-prompt').hide();
  }
  
  function handleDefaultLocation(zip) {
    if($('#defLocChkBox').attr('checked') == "checked") {
        setCookie("zip",zip,365);
    }
    else {
        setCookie("zip",zip,null);
    }
    //closeLocationEditPanel();
    window.location.reload();
  }
  
  
  
  function setCookie(key, value, expiryDays) {
    inspCookieKey = 'inspcookie';
    inspCookieValue = getCookie(inspCookieKey);    
    inspCookieValue = removeKeyFromInspCookie(key,inspCookieValue);    
    if(inspCookieValue.length != 0) {
      inspCookieValue += "&";
    }
    
    if(value != null && value.length != 0) {
      inspCookieValue += key+"="+value;
      // no expiration date
      if(expiryDays != null) {
        var exDate = new Date();
        exDate.setTime(exDate.getTime() + (expiryDays*24*60*60*1000));
        inspCookieValue += "; expires=" + exDate.toUTCString();
      }
      inspCookieValue += "; domain=" + location.hostname;
      cookie = inspCookieKey+"="+inspCookieValue+";  path=/";
    }
    document.cookie = cookie;
  }
  
  function getCookie(c_name)
  {
    cookies=document.cookie.split(";");
    for (i=0;i<cookies.length;i++)
    {
    name=cookies[i].substr(0,cookies[i].indexOf("="));
    value=cookies[i].substr(cookies[i].indexOf("=")+1);
    name=name.replace(/^\s+|\s+$/g,"");
      if (name==c_name) {
        return value;
      }
    }
    return "";
  }
  
  function removeKeyFromInspCookie(key,cookieValue) {
    regExp = new RegExp(key + "=\[^&\]+", "gi");
    cookieValue = cookieValue.replace(regExp,"");
    cookieValue = cookieValue.replace(/^&+/,"");
    cookieValue = cookieValue.replace(/&+$/,"");
    return cookieValue;
  }
  
  function getZip(city) {
    
      usZipMatches = city.match(/^\d{5}$/gi);
    canadaZipMatches = city.match(/^[a-zA-Z]\d[a-zA-Z]\s\d[a-zA-Z]\d$/gi);
    
    if(usZipMatches != null && city.length == 5) {
      return usZipMatches[0];
    } else if(canadaZipMatches != null && city.length == 7) {
      return canadaZipMatches[0].toUpperCase();
    } else {
      return null;
    }
       
  }
  
  $("#locFinder").click(function() {
    try {
      if(navigator.geolocation) {
        navigator.geolocation.getCurrentPosition(getLocationFromGoogle, handleError, {maximumAge:600000, timeout:5000, enableHighAccuracy: true});
      }
    }
    catch(err) { 
      //alert(err.message);
    }
    })
      
   function handleError(error) {
     // alert("An unexpected error occurred [" + error.code + "]: " + error.message); 
     	if(error.code == 1) {
          alert("Please enable the location service for the device's browser(Safari/FireFox/IE)");
     	} 
     //else if( error.code == 2) {
     //  alert("Error: Position is unavailable!");
     //   }
   }
    
   function getLocationFromGoogle(position) {
     
      try {
	var latitude = position.coords.latitude;	
	var longitude = position.coords.longitude;
      
        var geocoder = new google.maps.Geocoder();	  
	var latlng = new google.maps.LatLng(latitude, longitude);
       
        geocoder.geocode( {'latLng':latlng}, function(results, status) {				
          if(status == google.maps.GeocoderStatus.OK) {

            var addrs = getAddresses(results, true);
            if(typeof addrs[0] == "undefined") {
              //console.log('undefined address');
                 locationError();
            }
            if(addrs.length > 0) {
              var address = addrs[0];
              updateLocation(address.city + ',' + address.state);
              handleDefaultLocation(address.zipCode);
            }// if results[3] is there
          }	
        } );
     }
      catch(err) {
        // console.log(err.message);
        //alert(err.message);
      }
  }
  
  // Takes a Google geocode result array and returns an array of addresses
  // Only complete addresses with city, state, country and zip are returned
  function getAddresses(results, zip_required) {
    var ret = [];
    
    if ($.isArray(results)) {
      for (var i=0; i<results.length; i++) {
        var addr = getAddr(results[i].address_components);
        if (addr.city && addr.state && addr.country && (!zip_required || addr.zipCode)) {
          addr.latitude = results[i].geometry.location.lat();
          addr.longitude = results[i].geometry.location.lng();
          ret.push(addr);
        }
      }
    }
    return ret;
  }
  
  function getAddr(address_components) {
    var city, state, zipCode, country;
  
    out:
    for (var i=0; i<address_components.length; i++) {
      var component = address_components[i];
      for (var j=0; j<component.types.length; j++) {
        var type = component.types[j];
        switch(type) {
          case 'locality':
            city = component.long_name;
            continue out;
          case 'administrative_area_level_1':
            state = component.short_name;
            continue out;
          case 'country':
            country = component.short_name;
            continue out;
          case 'postal_code_prefix':
            continue out;
          case 'postal_code':
            zipCode = component.long_name;
            continue out;
        }
      };
    };
    
    return {city: city, state: state, zipCode: zipCode, country: country};
  }
  
  function getAddress(address_components) {
    var addr = getAddr(address_components);
    var address = new Array(2);
    address[0] = addr.city + ',' + addr.state;
    address[1] = addr.zipCode;
    return address;
  }
    
  
    function locationError() {
           $('#location-prompt').html("Location not found");
           $('#location-prompt').css("color", "red");
          $('#location-prompt').show();
      return false;
  }
  

  
</script>


	
        
          



