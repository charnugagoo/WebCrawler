<script>
YAHOO.namespace("yhelp");
var yhelpCallback = {};
var yhelpCrumb = "71qBcl5d9cO";
var yhelpPostData = "";
var yhelpRequestUrl = "";
var yhelpRequest = "";
var yhelpPropertyDirectory = "us/yahoo/helpcentral/";

var yhelpSetMyhelpStateCallback = {};

function yhelp_set_myhelp_state(part, state, dir, block){
	yhelpPostData = "crumb=" + encodeURIComponent(yhelpCrumb);
	yhelpPostData = yhelpPostData + "&state=" + encodeURIComponent(state); 
	yhelpPostData = yhelpPostData + "&part=" + encodeURIComponent(part); 
	yhelpPostData = yhelpPostData + "&dir=" + encodeURIComponent(dir); 
	yhelpPostData = yhelpPostData + "&block=" + encodeURIComponent(block); 
	yhelpRequestUrl = "/js/setmyhelpstate.html";
	yhelpRequest = YAHOO.util.Connect.asyncRequest('POST', yhelpRequestUrl, yhelpSetMyhelpStateCallback, yhelpPostData);
}


</script>

<script type="text/javascript">
	/* Functions to support more services block */
	function onLoadMoreServices() {
		collapseMoreServices();
	}
	function expandMoreServices() {
		var moreServicesDiv = document.getElementById("morehelpservices");
		var expandMoreServicesButton = document.getElementById("expandmorehelpservicestoggle");
		moreServicesDiv.style.display = 'block';
		expandMoreServicesButton.style.display = 'none';

	}
	function collapseMoreServices() {
		var moreServicesDiv = document.getElementById("morehelpservices");
		var expandMoreServicesButton = document.getElementById("expandmorehelpservicestoggle");
		moreServicesDiv.style.display = 'none';
		expandMoreServicesButton.style.display = 'block';
	}

	/* Functions to support Helpcentral blocks */

	function flipHelpCentralBlock(inIdIdx) {
		var helpCentralHeaderDiv = document.getElementById('helpcentralheader'+inIdIdx);
		var helpCentralBlockDiv = document.getElementById('helpcentralblock'+inIdIdx);

		if (helpCentralHeaderDiv.className=='contntblck-collaps') {
			/* The block is collapsed so expand it */
			helpCentralHeaderDiv.className='contntblck-expnd';
			helpCentralBlockDiv.style.display = 'block';
		} else {
			/* The block is expanded so collapse it */
			helpCentralHeaderDiv.className='contntblck-collaps';
			helpCentralBlockDiv.style.display = 'none';
		}
	}
</script>
