<script>
YAHOO.yhelp.panel2 = new YAHOO.widget.Overlay("suprprop", { fixedcenter:true, visible:false,width:"500px" } );
YAHOO.yhelp.panel2.render();
var yhelpMPOverlaySuccess = function(o) {
	var yhelpMPOverlayText = eval("(" + o.responseText + ")");
	yhelpMPOverlayText = '<div class="closewindow" style="text-align:right;"><a href="javascript:;"><img src="http://yui.yahooapis.com/2.5.2/build/container/assets/close12_1.gif" onClick="YAHOO.yhelp.panel2.hide();"> </a></div><div>' + yhelpMPOverlayText + '</div>';
	document.getElementById('suprprop').innerHTML = yhelpMPOverlayText;
	YAHOO.yhelp.panel2.show();	
};
var yhelpMPOverlayFailure = function(o) {

};
var yhelpMPOverlayCallback = {
	success:yhelpMPOverlaySuccess,
	failure:yhelpMPOverlayFailure
}
function yhelpMPOverlay(mp) {
	yhelpRequestUrl = "/ws/mpoverlay/v1/?mp=" + mp;
	var mpRequest = YAHOO.util.Connect.asyncRequest('GET', yhelpRequestUrl, yhelpMPOverlayCallback);
}
</script>
