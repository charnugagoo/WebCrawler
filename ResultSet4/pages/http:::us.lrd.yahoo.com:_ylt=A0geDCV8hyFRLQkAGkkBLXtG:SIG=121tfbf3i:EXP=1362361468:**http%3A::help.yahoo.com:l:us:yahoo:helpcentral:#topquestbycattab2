<script>

	function flip_searches(){
		var el_flip_searches = document.getElementById('flip_searches');
		var el_recent_searches = document.getElementById('recent_searches');
		if(el_flip_searches.className == "expnd"){
			el_recent_searches.style.display = 'none';
			el_flip_searches.className = 'collaps';
			yhelp_set_myhelp_state("search", "0", "", "");
		}else{
			el_recent_searches.style.display = 'block';
			el_flip_searches.className = "expnd";
			yhelp_set_myhelp_state("search", "1", "", "");
		}
	}
	function flip_articles(){
		var el_flip_articles = document.getElementById('flip_articles');
		var el_recent_articles = document.getElementById('recent_articles');
		if(el_flip_articles.className == "expnd"){
			el_recent_articles.style.display= 'none';
			el_flip_articles.className = 'collaps';
			yhelp_set_myhelp_state("articles", "0", "", "");
		}else{
			el_recent_articles.style.display= 'block';
			el_flip_articles.className = "expnd";
			yhelp_set_myhelp_state("articles", "1", "", "");
		}
	}

	var yhelpClearRecentViewsSuccess = function(o) {
		document.getElementById('recent_articles_listing').innerHTML = "No Recently Viewed Articles";
		document.getElementById('myhelpcount_articles').innerHTML = "0";
		document.getElementById('recent_views_clear').style.display="none";
		document.getElementById('recent_articles').style.display="none";
		document.getElementById('flip_articles').className = "collaps";
	}

	var yhelpClearRecentViewsCallback = { success:yhelpClearRecentViewsSuccess };

	function yhelp_clear_recent_views(){
		yhelpPostData = "crumb=" + encodeURIComponent(yhelpCrumb);
		yhelpRequestUrl = "/js/clearrecentviews.html";
		yhelpRequest = YAHOO.util.Connect.asyncRequest('POST', yhelpRequestUrl, yhelpClearRecentViewsCallback, yhelpPostData);
	}

	var yhelpClearSaveSuccess = function(o) {
		document.getElementById('recent_articles_listing').innerHTML = "No saved articles.";
		document.getElementById('myhelpcount_articles').innerHTML = "0";
		document.getElementById('recent_articles_clear').style.display="none";
		document.getElementById('recent_articles').style.display="none";
		document.getElementById('flip_articles').className = "collaps";
	}

	var yhelpClearSaveCallback = { success:yhelpClearSaveSuccess };

	function yhelp_clear_saved(){
		yhelpPostData = "crumb=" + encodeURIComponent(yhelpCrumb);
		yhelpRequestUrl = "/js/clearsavedarticles.html";
		yhelpRequest = YAHOO.util.Connect.asyncRequest('POST', yhelpRequestUrl, yhelpClearSaveCallback, yhelpPostData);
	}

	var yhelpClearSearchesSuccess = function(o) {
		document.getElementById('recent_searches_listing').innerHTML = "No recent searches";
		document.getElementById('myhelpcount_searches').innerHTML = "0";
		document.getElementById('recent_searches_clear').style.display="none";
		document.getElementById('recent_searches').style.display="none";
		document.getElementById('flip_searches').className = "collaps";
	}

	var yhelpClearSearchesCallback = { success:yhelpClearSearchesSuccess };

	function yhelp_clear_searches(){
		yhelpPostData = "crumb=" + encodeURIComponent(yhelpCrumb);
		yhelpRequestUrl = "/js/clearrecentsearches.html";
		yhelpRequest = YAHOO.util.Connect.asyncRequest('POST', yhelpRequestUrl, yhelpClearSearchesCallback, yhelpPostData);
	}

	YAHOO.util.Event.addListener("click2flip_searches", "click", flip_searches);
	YAHOO.util.Event.addListener("click2flip_articles", "click", flip_articles);
	if(document.getElementById('clear_recent_searches'))
	{
		YAHOO.util.Event.addListener("clear_recent_searches", "click", yhelp_clear_searches);
	}
	if(document.getElementById('clear_recent_articles'))
	{
		YAHOO.util.Event.addListener("clear_recent_articles", "click", yhelp_clear_saved);
	}
	if(document.getElementById('clear_recent_views'))
	{
		YAHOO.util.Event.addListener("clear_recent_views", "click", yhelp_clear_recent_views);
	}
</script><div style="clear:both";></div><div id='articleContentBlocksDiv' class='contntblck rt'>
