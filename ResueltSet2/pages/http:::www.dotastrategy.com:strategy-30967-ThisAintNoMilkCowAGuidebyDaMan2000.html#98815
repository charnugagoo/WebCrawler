        <tr>
            <td style="font-size: 1px; height: 64px">&nbsp;</td>
            <td valign="top" align="left" style="font-size: 1px; height: 64px"><img title="Bracer" height="64" alt="Bracer" width="64" border="0" src="http://www.dotastrategy.com/images/item/bracer.gif" /></td>
            <td style="font-size: 1px; height: 64px">&nbsp;</td>
