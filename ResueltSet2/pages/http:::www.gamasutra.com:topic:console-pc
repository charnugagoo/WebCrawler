<script>
$(function () {
    SetupNotifications();
});

SetupNotifications = function () {
    //setup close button in msgBox
    $("#headerCloseButton").click(function (e) {
        e.preventDefault();
        CloseMsg();
    });
}

DisplayMsg = function (sMsg) {
    //set the message text
    $("#headerText").html(sMsg);
    //show the message
    $('#headerBox').slideDown();
}

CloseMsg = function () {
    //hide the message
    $('#headerBox').slideUp();
    //clear msg text
    $("#headerText").val("");
}
</script>

