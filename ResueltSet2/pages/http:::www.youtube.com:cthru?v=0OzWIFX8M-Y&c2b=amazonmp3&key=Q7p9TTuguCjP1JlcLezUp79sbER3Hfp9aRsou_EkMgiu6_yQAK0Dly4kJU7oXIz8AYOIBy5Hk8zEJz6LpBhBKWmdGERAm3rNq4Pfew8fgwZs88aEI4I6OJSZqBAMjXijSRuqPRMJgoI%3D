<script type="text/javascript" src="http://z-ecx.images-amazon.com/images/G/01/javascripts/lib/swfobject/swfobject-2.2.min._V221898538_.js"></script>
<script type="text/javascript">
amznJQ.onReady('jQuery', function () {

var retriedCreateMp3Interface = false;
var createMp3Interface = function($) {
try {
    var swfURL = "http://g-ecx.images-amazon.com/images/G/01/zeitgeist/mp3player/swf/zgMp3Player-1.0._V212274098_.swf";
    var mp3PlayerElemId = "mp3FlashPlayer";
    var playerHeight = 1; // should be 1.  Flash objects initiated with 0 dimensions will not get initialized
    var playerWidth = 1;  // should be 1 as well.
    var minFlashVersion = "9.0.0";
    var expressInstallSwfurl = null;  // not used
    var flashVars = {
      protocol: window.location.protocol.replace(":",""),
      sessionId: "191-4499359-9445509" // used for telemetry logging
    };
    var params = {
      allowScriptAccess: "always" // allow JavaScript to call the MP3 player
    };

    if (jQuery("#" + mp3PlayerElemId).length === 0) {
        if (jQuery(".mp3Player:first").length !== 0) {
            jQuery(".mp3Player:first").append('<div id="' + mp3PlayerElemId + '"></div>');
        } else {
            jQuery("body").append('<div id="' + mp3PlayerElemId + '"></div>');
        }
    }

    try {
        var flashVersion = swfobject.getFlashPlayerVersion();
    } catch (e) {
        //swfobject not defined
        return;
    }

    swfobject.embedSWF(swfURL, mp3PlayerElemId, playerHeight, playerWidth, minFlashVersion, expressInstallSwfurl, flashVars, params);

    if (!window.Mp3Interface) {
        window.Mp3Interface = {
            flashPlayerId: mp3PlayerElemId,
            playerList: [],
            currentPlayer: null,
            muted: false,
            initialized: false,
            retriedInit: false,


            registerPlayer: function(player) {
                Mp3Interface.playerList.push(player);

                if (Mp3Interface.currentPlayer === null) {
                    Mp3Interface.currentPlayer = player;
                }

                if (this.initialized) {
                    player.callback('onPlayerInit');
                }
            },


            playSong: function(url, requestingPlayer) {
                Mp3Interface.setCurrentPlayer(requestingPlayer);
                swfobject.getObjectById(Mp3Interface.flashPlayerId).playSong(url);
            },
            playSongAsin: function(asin, realm, requestingPlayer) {
                Mp3Interface.setCurrentPlayer(requestingPlayer);
                swfobject.getObjectById(Mp3Interface.flashPlayerId).playSongAsin(asin, realm);
            },
            pauseSong: function(requestingPlayer) {
                if (requestingPlayer) {
                    Mp3Interface.setCurrentPlayer(requestingPlayer);
                }
                swfobject.getObjectById(Mp3Interface.flashPlayerId).pauseSong();
            },


            getFlashState: function() {
                try {
                  return swfobject.getObjectById(Mp3Interface.flashPlayerId).getState();
                } catch (e) {
                  return undefined;
                }
            },
            isInitialized: function() {
                return this.initialized;
            },
            backupInit: function () {
                if (this.initialized) {
                    return;
                }

                try {
                    setTimeout(function() {
                        if (swfobject && swfobject.getObjectById(Mp3Interface.flashPlayerId) && Mp3Interface.getFlashState()) {
                            Mp3Interface.callback("onPlayerInit");
                        } else {
                            Mp3Interface.backupInit();
                        }
                    },100);
                } catch (e)  {
                    if (Mp3Interface.retriedInit) {
                        return ;
                    } else {
                        setTimeout(function () {
                            Mp3Interface.retriedInit = true;
                            Mp3Interface.backupInit();
                        }, 500);
                    }
                }
            },


            callback: function(event) {
                switch (event) {
                    case 'onPlayerInit':
                        for (var i in Mp3Interface.playerList) {
                            var player = Mp3Interface.playerList[i];
                            player.callback(event);
                        }

                        this.initialized = true;
                        break;
                    default:
                        Mp3Interface.currentPlayer.callback(event);
                        break;
                }
            },


            play: function(asin, requestingPlayer) {
                Mp3Interface.setCurrentPlayer(requestingPlayer);
                Mp3Interface.currentPlayer.play(asin);
            },
            playTrack: function(asin, requestingPlayer) {
                Mp3Interface.setCurrentPlayer(requestingPlayer);
                Mp3Interface.currentPlayer.playTrack(asin);
            },
            playAll: function (requestingPlayer) {
                Mp3Interface.setCurrentPlayer(requestingPlayer);
                Mp3Interface.currentPlayer.playAll();
            },
            playNext: function (requestingPlayer) {
                Mp3Interface.setCurrentPlayer(requestingPlayer);
                Mp3Interface.currentPlayer.playNext();
            },
            playPrevious: function (requestingPlayer) {
                Mp3Interface.setCurrentPlayer(requestingPlayer);
                Mp3Interface.currentPlayer.playPrevious();
            },


            setVolume: function (volumeLevel) {
                if (volumeLevel > 0 && Mp3Interface.muted) {
                    Mp3Interface.toggleMute();
                }

                swfobject.getObjectById(Mp3Interface.flashPlayerId).setVolume(volumeLevel);
                Mp3Interface.setSharedVolume(volumeLevel);

                for (var i in Mp3Interface.playerList) {
                    var player = Mp3Interface.playerList[i];
                    player.setVolume(volumeLevel);
                }
            },
            initSharedVolume: function () {
                Mp3Interface.setVolume(Mp3Interface.getSharedVolume());
            },
            getSharedVolume: function () {
                return swfobject.getObjectById(Mp3Interface.flashPlayerId).getSharedObject('masterVolume') ? swfobject.getObjectById(Mp3Interface.flashPlayerId).getSharedObject('masterVolume') : 1;
            },
            setSharedVolume: function (volumeLevel) {
                swfobject.getObjectById(Mp3Interface.flashPlayerId).setSharedObject('masterVolume', volumeLevel);
            },
            getSharedCookie: function (cookieName) {
                return swfobject.getObjectById(Mp3Interface.flashPlayerId).getSharedObject(cookieName) ? swfobject.getObjectById(Mp3Interface.flashPlayerId).getSharedObject(cookieName) : null;
            },
            setSharedCookie: function (cookieName, cookieValue) {
                swfobject.getObjectById(Mp3Interface.flashPlayerId).setSharedObject(cookieName, cookieValue);
            },
            toggleMute: function () {
                Mp3Interface.muted = !Mp3Interface.muted;

                if (Mp3Interface.muted) {
                    var tmpVolume = Mp3Interface.getSharedVolume();
                    Mp3Interface.setVolume(0);
                    Mp3Interface.setSharedVolume(tmpVolume);
                } else {
                    Mp3Interface.setVolume(Mp3Interface.getSharedVolume());
                }

                for (var i in Mp3Interface.playerList) {
                    var player = Mp3Interface.playerList[i];
                    player.setMute(Mp3Interface.muted);
                }
            },


            setCurrentPlayer: function(requestingPlayer) {
                if (Mp3Interface.currentPlayer && Mp3Interface.currentPlayer !== requestingPlayer) {
                    Mp3Interface.pauseSong();
                    Mp3Interface.currentPlayer.reset();
                    Mp3Interface.currentPlayer = requestingPlayer;
                }
            }
        };
    }

    Mp3Interface.backupInit();
} catch (e) {
    if (retriedCreateMp3Interface) {
        return ;
    } else {
        setTimeout(function () {
            retriedCreateMp3Interface = true;
            createMp3Interface(jQuery);
        }, 500);
    }
}
};

createMp3Interface(jQuery);
});
</script>


<script type="text/javascript">

amznJQ.onReady('jQuery', function () {

var retriedCreateMp3Player = false;
var createMp3Player = function($) {
try {
    var minFlashVersion = "9.0.0";
    var mp3PlayerPrefix = "dm_";
    var mp3PlayerId = "#" + mp3PlayerPrefix + "mp3Player";
    var mp3Player = jQuery(mp3PlayerId);

    window.dm_dmusicPlayer = {
        playlist: [],
        mp3Tracks: $(mp3PlayerId + " a.mp3Asin"),
        playingAsin: null,
        loadAttempts: 0,
        maxRetries: 2,
        isPlayAll: false,
        volume: 1,
        muted: false,
        playURL: true,
        element: mp3Player,
        retriedRegisterPlayer: false,


        play: function(asin) {
            if (!asin) {
                return;
            }

            var inPlayState = $("#" + mp3PlayerPrefix + this.playingAsin).hasClass("playing")
                           || $("#" + mp3PlayerPrefix + this.playingAsin).hasClass("loading");
            var inPausedState = $("#" + mp3PlayerPrefix + this.playingAsin).hasClass("paused")
            if (this.playingAsin === asin && this.loadAttempts === 0 && inPlayState) {
                Mp3Interface.pauseSong(this);
                this.trackUI.pause();
            } else if (this.playingAsin === asin && this.loadAttempts === 0 && inPausedState) {
                Mp3Interface.pauseSong(this);
                this.trackUI.play($("#" + mp3PlayerPrefix + this.playingAsin));
            } else {
                this.playingAsin = asin;
                this.trackUI.play($("#" + mp3PlayerPrefix + this.playingAsin));
                if (this.playURL) {
                    Mp3Interface.playSong($("#" + mp3PlayerPrefix + this.playingAsin).attr("flashURL"), this);
                } else {
                    Mp3Interface.playSongAsin(asin, "", this);
                }
                Mp3Interface.initSharedVolume();
            }

            this.playingAsin = asin;
        },
        playTrack: function(asin) {
            this.isPlayAll = false;
            this.loadAttempts = 0;
            this.play(asin);
        },
        playAll: function () {
            if (this.playingAsin) {
                Mp3Interface.pauseSong(this);
            } else {
                this.playingAsin = null;
                this.isPlayAll = true;
                this.playNext();
            }
        },
        playNext: function () {
            this.isPlayAll = true;
            this.loadAttempts = 0;
            this.play(this.getNextSong());
        },
        playPrevious: function () {
            this.isPlayAll = true;
            this.loadAttempts = 0;
            this.play(this.getPreviousSong());
        },
        setVolume: function (volumeLevel) {
            this.volume = volumeLevel;
            var newTop = this.masterControlUI.heightOfVolumeBar - (volumeLevel * this.masterControlUI.heightOfVolumeBar);
            this.masterControlUI.setVolumeSlider(newTop);
        },
        setVolumeByPosition: function (newTop) {
            Mp3Interface.setVolume(this.masterControlUI.setVolumeSlider(newTop));
        },
        setMute: function (isMuted) {
            this.muted = isMuted;
            this.masterControlUI.updateMute();
        },
        getNextSong: function () {
            if (this.isPlayAll) {
                if (!this.playingAsin) {
                    return this.playlist[0];
                }
            }

            var foundCurrent = false;
            for (var i in this.playlist) {
                var songAsin = this.playlist[i];
                if (foundCurrent) {
                    return songAsin;
                }

                if (songAsin === this.playingAsin) {
                    foundCurrent = true;
                }
            }

            return null;
        },
        getPreviousSong: function () {
            var foundCurrent = false;
            var previousSongAsin = null;
            for (var i in this.playlist) {
                var songAsin = this.playlist[i];
                if (songAsin === this.playingAsin) {
                    return previousSongAsin;
                }

                previousSongAsin = songAsin;
            }

            return null;
        },

        registerPlayer: function () {
            var thisPlayer = this;

            try {
                if (!swfobject || mp3Player.hasClass("mp3PlayerEnabled") || (swfobject && !swfobject.hasFlashPlayerVersion(minFlashVersion))) {
                    jQuery(".flashWarning").addClass("flashDisabled");
                    return;
                }

                setTimeout(function () {
                    if (window.Mp3Interface) {
                        window.Mp3Interface.registerPlayer(thisPlayer);
                    } else {
                        thisPlayer.registerPlayer();
                    }
                }, 100);
            } catch (e) {
                if (thisPlayer.retriedRegisterPlayer) {
                    return ;
                } else {
                    setTimeout(function () {
                        thisPlayer.retriedRegisterPlayer = true;
                        thisPlayer.registerPlayer();
                    }, 500);
                }
            }
        },
        reset: function () {
            this.trackUI.reset();
            this.masterControlUI.reset();

            this.playingAsin = null;
            this.loadAttempts = 0;
        },

        init: function() {
            if (mp3Player.hasClass("mp3PlayerEnabled")) {
                return;
            }

            var thisPlayer = this;
            this.trackUI.parentPlayer = this;
            this.masterControlUI.parentPlayer = this;

            if (!swfobject || (swfobject && !swfobject.hasFlashPlayerVersion(minFlashVersion))) {
                jQuery(".flashWarning").addClass("flashDisabled");
                return;
            }

            this.mp3Tracks.each(function () {
                thisPlayer.playlist.push($(this).attr("asin"));
            });

            if (this.playlist.length === 0) { return; }

            this.mp3Tracks.click(function(e) {
                e.preventDefault();
                Mp3Interface.playTrack($(this).attr("asin"), thisPlayer);
            });

            $(".mp3MasterPlay", this.element).click(function(e) {
                e.preventDefault();
                Mp3Interface.playAll(thisPlayer);
            });

            $(".mp3PlayPrevious", this.element).click(function(e) {
                e.preventDefault();
                Mp3Interface.playPrevious(thisPlayer);
            });

            $(".mp3PlayNext, .mp3MasterPlayLabel", this.element).click(function(e) {
                e.preventDefault();
                Mp3Interface.playNext(thisPlayer);
            });

            $(".mp3MasterVolumeGroup .volumeIcon", this.element).click(function(e) {
                e.preventDefault();
                Mp3Interface.toggleMute();
            });

            $(".mp3MasterVolumeSelector", this.element).mousedown(function(e) {
                thisPlayer.masterControlUI.activeSlider = true;
            });

            $(".mp3MasterVolumeSelector", this.element).bind("mouseleave", function(e) {
                thisPlayer.masterControlUI.activeSlider = false;
            });

            $(".mp3MasterVolumeSelector", this.element).mousemove(function(e) {
                if (thisPlayer.masterControlUI.activeSlider) {
                    var offset = $(".mp3MasterVolumeGroup .volumeBar", thisPlayer.element).offset();
                    var scrollTop = $(window).scrollTop();
                    var top = e.clientY - offset.top + scrollTop;
                    thisPlayer.setVolumeByPosition(top);
                }
            });

            $(".mp3MasterVolumeSelector", this.element).mouseup(function(e) {
                if (thisPlayer.masterControlUI.activeSlider) {
                    var offset = $(".mp3MasterVolumeGroup .volumeBar", thisPlayer.element).offset();
                    var scrollTop = $(window).scrollTop();
                    var top = e.clientY - offset.top + scrollTop;
                    thisPlayer.setVolumeByPosition(top);

                    thisPlayer.masterControlUI.activeSlider = false;
                }
            });

            if ($.browser.msie) {
                $(".mp3MasterPlayGroup").addClass("ie");

                var volumeGroup = $(".mp3MasterVolumeGroup", thisPlayer.element);
                volumeGroup.hover(function() {
                    volumeGroup.addClass("hover");
                }, function() {
                    volumeGroup.removeClass("hover");
                });
            }

            mp3Player.addClass("mp3PlayerEnabled");
            
            if (Mp3Interface.isInitialized()) {  
              Mp3Interface.initSharedVolume();
            }

            this.syncFlashAndHTTPCookies();
        },

        syncFlashAndHTTPCookies: function () {
            
            if (!Mp3Interface.isInitialized()) {
                setTimeout(function () {
                  window.dm_dmusicPlayer.syncFlashAndHTTPCookies();
                }, 500);
                return;
            }

            var getCookie = function(c_name) {
                if (document.cookie.length > 0) {
                    var c_start = document.cookie.indexOf(c_name + "=");
                    if (c_start != -1) {
                        c_start=c_start + c_name.length+1;
                        var c_end = document.cookie.indexOf(";",c_start);
                        if (c_end == -1) {
                            c_end = document.cookie.length;
                        }

                        return document.cookie.substring(c_start,c_end);
                    }
                }
                return null;
            };

            function getAmdPluginValue() {
                var value = null;
                if (window.ActiveXObject) {
                    try {
                        amd = new ActiveXObject("Amazon.AmazonMP3DownloaderPlugin");
                        value = amd ? amd.version : null;
                    } catch(e) {}
                } else if(navigator.plugins) {
                    navigator.plugins.refresh();
                    amd = navigator.plugins["AmazonMP3DownloaderPlugin"];
                    var regex = /.*?([0-9]+[.][0-9]+[.][0-9]+)$/;
                    value = (amd && amd.description.match(regex)) ? amd.description.match(regex)[1] : null;
                }
                return value;
            }

            var createBrowserCookie = function(domain, name, value) {
                var date = new Date();
                date.setTime(date.getTime()+(10*365*24*60*60*1000)); 
                var expires = "; expires="+date.toGMTString();
                return name+"="+value+expires+"; path=/; domain=" + domain;
            };

            var cookieName = 'dmusic_download_manager_enabled';
            var flashAMDCookieValue = Mp3Interface.getSharedCookie(cookieName);

            var pluginAmdValue = getAmdPluginValue();
            var domain = ".amazon.com";

            if (pluginAmdValue != null) {
                Mp3Interface.setSharedCookie(cookieName, pluginAmdValue);
                document.cookie = createBrowserCookie(domain, cookieName, pluginAmdValue);
            } else {
                var httpAMDCookieValue = getCookie(cookieName);
                if(flashAMDCookieValue === null && httpAMDCookieValue !== null) {
                    Mp3Interface.setSharedCookie(cookieName, httpAMDCookieValue);
                } else if(flashAMDCookieValue == "uninstalled") {
                    var expires = "; expires=Thu, 01-Jan-1970 00:00:01 GMT";
                    document.cookie = cookieName+"="+flashAMDCookieValue+expires+"; path=/; domain=" + domain;
                } else if(flashAMDCookieValue !== null && 
                          flashAMDCookieValue != "uninstalled" &&
                          httpAMDCookieValue === null) {
                    document.cookie = createBrowserCookie(domain, cookieName, flashAMDCookieValue);
                }
            }
        },

        trackUI: {
            parentPlayer: null, //gets set by init()
            play: function(asin) {
                if (this.parentPlayer.loadAttempts === 0) {
                    this.parentPlayer.masterControlUI.play(asin);
                    this.parentPlayer.mp3Tracks.removeClass("playing paused loading error");
                    asin.addClass('playing');
                }
            },
            pause: function() {
                this.parentPlayer.mp3Tracks.removeClass("playing paused loading error");
                $("#" + mp3PlayerPrefix + this.parentPlayer.playingAsin).addClass('paused');
                this.parentPlayer.masterControlUI.pause();
            },
            loading: function() {
                this.parentPlayer.mp3Tracks.removeClass("playing paused loading error");
                $("#" + mp3PlayerPrefix + this.parentPlayer.playingAsin).addClass('loading');
                this.parentPlayer.masterControlUI.loading();
            },
            reset: function() {
                this.parentPlayer.mp3Tracks.removeClass("playing paused loading error");
            }
        },

        masterControlUI: {
            parentPlayer: null, //gets set by init()
            masterUIStates: "playing paused loading error hasNext hasPrevious",
            playGroup: mp3Player.find(".mp3MasterPlayGroup"),
            volumeGroup: mp3Player.find(".mp3MasterVolumeGroup"),
            volumeSlider: mp3Player.find(".mp3MasterVolumeGroup .volumeSlider"),
            volumeBackground: mp3Player.find(".mp3MasterVolumeGroup .background"),
            heightOfVolumeBar: mp3Player.find(".mp3MasterVolumeGroup .volumeBar").css("height").replace("px",""),
            sliderStartTop: 4,
            activeSlider: false,
            play: function(playingAsin) {
                this.playGroup.find(".songTitle").html(
                    this.truncateTitle(playingAsin.parents("tr").find(".songTitle"))
                );
                this.playGroup.removeClass(this.masterUIStates);
                this.playGroup.addClass("playing");

                this.setNextAndPrevious();
            },
            pause: function() {
                this.playGroup.removeClass(this.masterUIStates);
                this.playGroup.addClass("paused");

                this.setNextAndPrevious();
            },
            loading: function() {
                this.playGroup.removeClass(this.masterUIStates);
                this.playGroup.addClass("loading");

                this.setNextAndPrevious();
            },
            error: function() {
                this.playGroup.removeClass(this.masterUIStates);
                this.playGroup.addClass("error");

                this.setNextAndPrevious();
            },
            reset: function() {
                this.playGroup.removeClass(this.masterUIStates);
            },
            setNextAndPrevious: function() {
                if (this.parentPlayer.getNextSong()) {
                    this.playGroup.addClass("hasNext");
                }
                if (this.parentPlayer.getPreviousSong()) {
                    this.playGroup.addClass("hasPrevious");
                }
            },
            updateMute: function() {
                if (this.parentPlayer.muted) {
                    this.volumeGroup.addClass("mute");
                } else {
                    this.volumeGroup.removeClass("mute");
                }
            },
            setVolumeSlider: function(newTop) {
                var volumePercent = (this.heightOfVolumeBar - newTop) / (this.heightOfVolumeBar - this.sliderStartTop);

                if (volumePercent > 1) {
                    volumePercent = 1;
                } else if (volumePercent < 0) {
                    volumePercent = 0;
                    newTop = this.heightOfVolumeBar;
                }

                if (newTop < this.sliderStartTop) {
                    newTop = this.sliderStartTop;
                }

                this.volumeSlider.css("top", newTop);
                this.volumeBackground.css("top", newTop);

                return volumePercent;
            },

            truncateTitle: function(titleElement, maxLen) {
                var elementClone = titleElement.clone();
                var title = elementClone.find("a");
                var hasLink = false;

                if (title.length !== 0) {
                    title = title.html();
                    hasLink = true;
                } else {
                    title = elementClone.html();
                }

                var maxLength = (maxLen) ? maxLen : 25;
                var newTitle = (title.length > maxLength) ? title.substring(0,maxLength) + "..." : title;

                if (hasLink) {
                    elementClone.find("a").html(newTitle);
                } else {
                    elementClone.html(newTitle);
                }

                return elementClone.html();
            }
        },

        callback: function(event) {
            var thisPlayer = this;
            var activeAsin = $("#" + mp3PlayerPrefix + this.playingAsin);
            switch (event) {
                case 'onIOError':
                case 'onInvalidUrl':
                    if (this.playingAsin && this.loadAttempts < this.maxRetries) {
                        this.trackUI.loading();
                        setTimeout(function () {
                            thisPlayer.loadAttempts++;
                            thisPlayer.play(thisPlayer.playingAsin);
                        }, 1000);
                    } else {
                        this.masterControlUI.error();
                        this.loadAttempts = -1;

                        this.mp3Tracks.removeClass("playing paused loading error");

                        if (this.isPlayAll && this.getNextSong()) {
                            this.loadAttempts = 0;
                            this.playNext();
                        } else {
                            this.playingAsin = null;
                        }
                    }

                    break;
                case 'onLoad':
                    if (this.loadAttempts !== 0) {
                        break;
                    }
                case 'onPlay':
                    this.loadAttempts = 0;
                    this.trackUI.play(activeAsin);
                    break;
                case 'onPause':
                    this.trackUI.pause();
                    break;
                case 'onComplete':
                    this.mp3Tracks.removeClass("playing paused loading error");
                    if (!this.isPlayAll || (this.isPlayAll && !this.getNextSong())) {
                        this.playingAsin = null;
                        this.isPlayAll = false;
                    }

                    if (this.isPlayAll) {
                        this.playNext();
                    } else {
                        this.masterControlUI.reset();
                    }
                    break;
                case 'onPlayerInit':
                    this.init();
                    break;
                default:
                    break;
            }
        }
    };

    dm_dmusicPlayer.registerPlayer();
} catch (e) {
    if (retriedCreateMp3Player) {
        return ;
    } else {
        setTimeout(function () {
            retriedCreateMp3Player = true;
            createMp3Player(jQuery);
        }, 500);
    }
}
};

createMp3Player(jQuery);
});

</script>
<div id='touPlayerFooter' style='text-align:center;margin-bottom:0.5em;font-size:xx-small;'>
  Sold by Amazon Digital Services, Inc..  Additional taxes may apply. By placing your order, you agree to our <a href="/gp/dmusic/tou/mp3.html/ref=dm_dp_eula/191-4499359-9445509?ie=UTF8&amp;%20pop-up=1" onClick="return amz_js_PopWin(this.href,'AmazonHelp','width=800,height=600,resizable=1,scrollbars=1,toolbar=1,status=1');">Terms of Use</a>.
</div>

    </div> 
</div> 



























