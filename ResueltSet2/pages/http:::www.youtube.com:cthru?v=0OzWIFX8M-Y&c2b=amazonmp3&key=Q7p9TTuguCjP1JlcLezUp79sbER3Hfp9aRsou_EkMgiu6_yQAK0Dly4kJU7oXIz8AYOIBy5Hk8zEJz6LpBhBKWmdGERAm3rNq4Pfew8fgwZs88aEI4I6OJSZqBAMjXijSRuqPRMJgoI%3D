<script type="text/javascript">
if(!window.amznMusic){
    amznMusic = {};
}

(function(){
    var amp = null;
    var browser = "Safari";
    var baseUrl = "https://www.amazon.com/gp/dmusic/mp3/player/ref=dm_ty_cp/191-4499359-9445509";
    var target = "amazonMP3Player";
    
    var windowFeatures = function(){
        if(browser === "Netscape"){
            return "resizable,scrollbars,status,location,toolbar,centerscreen";
        }
        
        if(browser === "Safari"){
            return "";
        }
        
        if(browser === "Internet Explorer"){
            return "location,resizable,scrollbars,status,toolbar"; 
        }
        
        return "resizable=yes,scrollbars=yes,status=yes,location=yes,toolbar=yes";
    }();
    
    amznMusic.launcher = {
        close: function(){
            if(amp && amp.close){
                amp.close();
                amp = null;
            }
        },
        
        focus: function(){
            if(amp && amp.focus){
                amp.focus();
            }
        },
        
        open: function(){
            if(amp && !amp.closed){
                amznMusic.launcher.focus();
                return;
            }
            amp = window.open(baseUrl, target, windowFeatures);
            amznMusic.launcher.focus();
        },
        
        openAfterPurchase : function(fromTYPOrderId){
            if(amp && !amp.closed){
                amznMusic.launcher.focus();
                return;
            }
    
            var url = baseUrl + "?fromTYPOrderId=" + fromTYPOrderId + "#latestPurchases";
            amp = window.open(url, target, windowFeatures);
            amznMusic.launcher.focus();
        }
    }
})();
</script>

  



