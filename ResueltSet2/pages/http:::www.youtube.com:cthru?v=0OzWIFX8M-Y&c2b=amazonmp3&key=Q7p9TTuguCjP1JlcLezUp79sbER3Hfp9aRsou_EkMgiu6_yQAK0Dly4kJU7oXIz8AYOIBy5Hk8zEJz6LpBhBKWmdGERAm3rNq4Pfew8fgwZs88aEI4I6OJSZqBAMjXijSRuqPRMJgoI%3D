        stateCache.ready(function(){
            if (stateCache.getTimestamp() < 1361150237) {
                stateCache.set('isLiked', jQuery("#amznLikeStateCache_19144993599445509_dp_asin_B001IAOQGK_isLiked").text()  == "true");
                stateCache.set('customerWhitelistStatus',  parseInt(jQuery("#amznLikeStateCache_19144993599445509_dp_asin_B001IAOQGK_customerWhitelistStatus").text()));
                stateCache.set('likeCount', parseInt(jQuery("#amznLikeStateCache_19144993599445509_dp_asin_B001IAOQGK_likeCount").text()));
                stateCache.set('commifiedLikeCount', jQuery("#amznLikeStateCache_19144993599445509_dp_asin_B001IAOQGK_commifiedLikeCount").text());
                stateCache.set('commifiedLikeCountMinusOne', jQuery("#amznLikeStateCache_19144993599445509_dp_asin_B001IAOQGK_commifiedLikeCountMinusOne").text());
                stateCache.set('ts', "1361150237");
            }
        if (!window.amznLikeStateCache) {
            window.amznLikeStateCache = {};
        }
        window.amznLikeStateCache["amznLikeStateCache_19144993599445509_dp_asin_B001IAOQGK"] = stateCache;
        });


        var amznLikeDiv = jQuery("#amznLike_B001IAOQGK");
        var stateCache = window.amznLikeStateCache["amznLikeStateCache_19144993599445509_dp_asin_B001IAOQGK"];
        amznLikeDiv.remove();

        var amznLike;
        amznLike = amznLikeDiv.amazonLike({
            context             : "dp",
            itemId              : "B001IAOQGK",
            itemType            : "asin",
            isLiked             : stateCache.get("isLiked"),
            customerWhitelistStatus : stateCache.get("customerWhitelistStatus"),
            isCustomerSignedIn  : false,
            isOnHover           : false,
            isPressed           : false,
            popoverWidth        : 335,
            popoverAlign        : "right",
            popoverOffset       : 0,
            sessionId           : "191-4499359-9445509",
            likeCount           : stateCache.get("likeCount"),
            commifiedLikeCount  : stateCache.get("commifiedLikeCount"),
            commifiedLikeCountMinusOne : stateCache.get("commifiedLikeCountMinusOne"),
            isSignInRedirect    : false,
            shareText 		    : "Share this item",
            onBeforeAttachPopoverCallback : function () {
                jQuery("#likeAndShareBar").append(amznLikeDiv).show();
            },
            spriteURL           : "http://g-ecx.images-amazon.com/images/G/01/x-locale/personalization/amznlike/amznlike_sprite_02._V196113939_.gif",
            buttonOnClass       : 'on',
            buttonOffClass      : 'off',
            buttonPressedClass  : 'pressed',
            popoverHTML         : "<div id="+'"'+"amazonLikePopoverWrapper_B001IAOQGK"+'"'+" class="+'"'+"amazonLikePopoverWrapper amazonLikeContext_dp"+'"'+" >"+String.fromCharCode(0x000D)+"<div class="+'"'+"amazonLikeBeak "+'"'+">&nbsp;</div>    <div class="+'"'+"amazonLikePopover"+'"'+">"+String.fromCharCode(0x000D)+"<div class="+'"'+"likePopoverError"+'"'+" style="+'"'+"display: none;"+'"'+">"+String.fromCharCode(0x000D)+"<span class="+'"'+"error"+'"'+" style="+'"'+"color: #900;"+'"'+"><strong>An error has occurred. Please try your request again.</strong></span>"+String.fromCharCode(0x000D)+"</div>"+String.fromCharCode(0x000D)+"<div class="+'"'+"likeOffPopoverContent"+'"'+" >"+String.fromCharCode(0x000D)+"<div>"+String.fromCharCode(0x000D)+"<div class="+'"'+"likeCountText likeCountLoadingIndicator"+'"'+" style="+'"'+"display: none;"+'"'+"><img src="+'"'+"http://g-ecx.images-amazon.com/images/G/01/javascripts/lib/popover/images/snake._V192571611_.gif"+'"'+" width="+'"'+"16"+'"'+" alt="+'"'+"Loading"+'"'+" height="+'"'+"16"+'"'+" border="+'"'+"0"+'"'+" /></div>"+String.fromCharCode(0x000D)+"<span style="+'"'+"font-weight: bold;"+'"'+"><a class="+'"'+"likeSignInLink"+'"'+" href="+'"'+"/gp/like/sign-in/sign-in.html/ref=pd_like_unrec_signin_nojs_dp/191-4499359-9445509?ie=UTF8&isRedirect=1&location=%2Fgp%2Flike%2Fexternal%2Fsubmit.html%2Fref%3Dpd_like_submit_like_unrec_nojs_dp%2F191-4499359-9445509%3Fie%3DUTF8%26action%3Dlike%26context%3Ddp%26itemId%3DB001IAOQGK%26itemType%3Dasin%26redirect%3D1%26redirectPath%3D%252Fgp%252Fproduct%252FB001IAOQGK%253Ftag%253Dyou09f-20&useRedirectOnSuccess=1"+'"'+">Sign in to like this</a>.</span>"+String.fromCharCode(0x000D)+"</div>"+String.fromCharCode(0x000D)+"<div class="+'"'+"spacer"+'"'+">Telling us what you like can improve your shopping experience. <a class="+'"'+"grayLink"+'"'+" href="+'"'+"/gp/help/customer/display.html/ref=pd_like_help_dp/191-4499359-9445509?ie=UTF8&nodeId=13316081#like"+'"'+">Learn more</a></div>"+String.fro