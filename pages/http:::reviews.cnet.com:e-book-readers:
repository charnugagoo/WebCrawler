<script type="text/javascript">
var ads_eventTimer; var ads_curOpen=0; var ads_toOpen=0; var ads_speed=10; var ads_length=5; var ads_active=false;
var ads_accordionDiv=document.getElementById("ads_dynamicShowcase_accordion");
var ads_accordionHeaders=ads_accordionDiv.getElementsByTagName("h3");
var ads_accordionMains=ads_accordionDiv.getElementsByTagName("div");
var ads_accordionEvents=ads_accordionDiv.getElementsByTagName("h4");
if(typeof(ads_accordionMains[ads_curOpen])!="undefined"){
	ads_accordionMains[ads_curOpen].style.height="0px";ads_accordionMains[ads_curOpen].style.display="block";
	ads_expandMain(ads_curOpen);
	for(var i=0;i<ads_accordionHeaders.length;i++){ads_accordionHeaders[i].thisIndex=i;ads_accordionHeaders[i].onmouseover=ads_accordion;}
}
function ads_accordion(){
	ads_toOpen=this.thisIndex;
	setTimeout(ads_accordionGo,500);
}
function ads_accordionGo(){
	if(ads_curOpen!=ads_toOpen && ads_active==false){
		ads_active=true;
		ads_collapseMain(ads_curOpen);
		ads_curOpen=ads_toOpen;
		ads_accordionMains[ads_curOpen].style.height="0px";
		ads_accordionMains[ads_curOpen].style.display="block";
		ads_expandMain(ads_curOpen);
	}
}
function ads_collapseMain(i){
	var curHeight=parseFloat(ads_accordionMains[i].style.height)-ads_length;
	if(curHeight>-1){
		ads_accordionMains[i].style.height=curHeight+"px";
		setTimeout(function(){ads_collapseMain(i)},ads_speed);
	}
}
function ads_expandMain(i){
	var curHeight=parseFloat(ads_accordionMains[i].style.height)+ads_length;
	if(curHeight<170){
		ads_accordionMains[i].style.height=curHeight+"px";
		setTimeout(function(){ads_expandMain(i)},ads_speed);
	}else{ //transition has completed
		ads_active=false;
		ads_eventFire();
	}
}
function ads_eventFire(){
	var currentTime=new Date();
	var hours=currentTime.getHours();
	if(hours<10){hours="0"+hours;}
	var minutes=currentTime.getMinutes();
	if(minutes<10){minutes="0"+minutes;}
	var seconds=currentTime.getSeconds();
	if(seconds<10){seconds="0"+seconds;}
	var month=currentTime.getMonth()+1;
	if(month<10){month="0"+month;}
	var day=currentTime.getDate();
	if(day<10){day="0"+day;}
	var year=currentTime.getFullYear();
	var dateTime=year+"."+month+"."+day+"."+hours+"."+minutes+"."+seconds;	
	var ads_curEventStr = ads_accordionEvents[ads_curOpen].id;
	ads_curEventStr = ads_curEventStr.replace(/t=[0-9,.]{19}/i, "t="+dateTime);
	document.getElementById("ads_dynamicShowcase_rollEvent").src=ads_curEventStr;
}
</script>
