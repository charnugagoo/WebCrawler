<script type="text/javascript">
AE.defer(function(){
	var userGuideTab= new AE.widget.tabView({
		tab:'tab',
		con:'user-content',
		parentNode:'user-info',
		switchEvent:'click',
		deplay:200,
		currentClass:'current-tab',
		defaultTab:1,
		canHideAll:false
	});
	
	var tabContainer = get('tab-container');
	userGuideTab.onTabSwitch.subscribe(function(eventName,args){
		//ab
		if(args[0] === 0){
			YUD.addClass(tabContainer,'user-guide-buyer');
			YUD.removeClass(tabContainer,'user-guide-supplier');
		}
		//ab
		if(args[0] === 1){
			YUD.addClass(tabContainer,'user-guide-supplier');
			YUD.removeClass(tabContainer,'user-guide-buyer');
		}
	});
});
</script>

