			function displayMoreIncluedKeywords(isMore){
			if(isMore == 1)
			{
				var div = document.getElementById("include_keywords_div");
				div.style.display = "none";
				
				div = document.getElementById("include_keywords_more_div");
				div.style.display = "";
				}
				else
				{
				var div = document.getElementById("include_keywords_div");
				div.style.display = "";
				
				div = document.getElementById("include_keywords_more_div");
				div.style.display = "none";
				}
			}
