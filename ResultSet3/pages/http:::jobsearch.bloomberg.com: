<script type="text/javascript">

var bq_timer = window.setInterval( function() {
    var bq_link = document.getElementById('nav_try_link');
    if ( bq_link ) {
        var base_domain = 'http://www.bloomberg.com';
        var header_link = document.getElementById('header_logo');
        if ( header_link && header_link.href ) {
            base_domain = header_link.href;
        }
        var bq_href = bq_link.getAttribute('href',2);
        bq_link.href = base_domain + bq_href;
        window.clearInterval(bq_timer);
    }
}, 250 );
</script>

