<script type='text/javascript'>

      function acrPopoverHover(e, h) {
        if(h) window.acrAsinHover = e;
        else if(window.acrAsinHover == e) window.acrAsinHover = null;
      }

  amznJQ.onReady('popover', function() {
    (function($) {
      if($.fn.acrPopover) return;
      var popoverConfig = {
        showOnHover:true,
        showCloseButton: true,
        width: null,
        location:'bottom',
        locationAlign:'left',
        locationOffset:[-20,0],
        paddingLeft: 15,
        paddingBottom: 5,
        paddingRight: 15,
        group: 'reviewsPopover',
        clone:false,
        hoverHideDelay:300
      };

      $.fn.acrPopover = function() {
          return this.each(function() {
            var $this = $(this);
            if(!$this.data('init')) {
              $this.data('init', 1);
              var getargs = $this.attr('getargs');
              var ajaxURL = '/gp/customer-reviews/common/du/displayHistoPopAjax.html?' +
                         '&ASIN=' + $this.attr('name') +
                         '&link=1' +
                         '&seeall=1' +
                         '&ref=' + $this.attr('ref') +
                         ((typeof getargs != 'undefined') ? ('&getargs=' + getargs) : '');
              var myConfig = $.extend(true, { destination: ajaxURL }, popoverConfig);
              $this.amazonPopoverTrigger(myConfig);

              var w = window.acrAsinHover;
              if(w && $(w).parents('.asinReviewsSummary').get(0) == this) {
                $this.trigger('mouseover.amzPopover');
                window.acrAsinHover = null;
              }
            }
        });
      };

      window.reviewHistPopoverConfig = popoverConfig;
      var jqInit = window.jQueryInitHistoPopovers = function(asin) {
        $('.acr-popover[name=' + asin + ']').acrPopover();
      };
      window.doInit_average_customer_reviews = jqInit;
      window.onAjaxUpdate_average_customer_reviews = jqInit;
      window.onCacheUpdate_average_customer_reviews = jqInit;
      window.onCacheUpdateReselect_average_customer_reviews = jqInit;

      amznJQ.onCompletion('amznJQ.criticalFeature', function() {
          setTimeout(function() { amznJQ.declareAvailable('acrPopover'); }, 10);
      });
    })(jQuery);
  });
  amznJQ.onReady('acrPopover', function() {
    jQuery('.acr-popover,#searchTemplate .asinReviewsSummary').each(function() {
      jQuery(this).acrPopover();
    });
  });
</script>


  


<script type="text/javascript">
var logPopover = false;
var popoverLogs = {};
var popoverTimeoutCount = 0; 
var popoverOtherErrorsCount = 0;
var popoverSingleSwatchCount = 0;
var popoverMultipleSwatchCount = 0;
var swatchHoverCount = 0;
var prevOnUnload = undefined;

function flushPopoverClientLog(event) {
  if (!logPopover) {
    return;
  }

  if (window.clientLogger && (swatchHoverCount || popoverTimeoutCount || popoverOtherErrorsCount || popoverSingleSwatchCount || popoverMultipleSwatchCount)) {
    popoverLogs['swatchHoverCount'] = swatchHoverCount;
    popoverLogs['popoverTimeoutCount'] = popoverTimeoutCount;
    popoverLogs['popoverOtherErrorsCount'] = popoverOtherErrorsCount;    
    popoverLogs['popoverSingleSwatchCount'] = popoverSingleSwatchCount;
    popoverLogs['popoverMultipleSwatchCount'] = popoverMultipleSwatchCount;    
    swatchHoverCount = 0;
    popoverTimeoutCount = 0;
    popoverSingleSwatchCount = 0;
    popoverMultipleSwatchCount = 0;
    popoverOtherErrorsCount = 0;
    clientLogger.sendCLOGEntry("bxindiapopover", "hover", popoverLogs);
  }

  if (prevOnUnload && !prevOnUnload.isFlushPopoverClientLog) {
    prevOnUnload(event);
  }
}
flushPopoverClientLog.isFlushPopoverClientLog = true;

function enablePopoverLogging() {
  try {
    if (!logPopover) {
      logPopover = true;
      prevOnUnload = window.onbeforeunload;
      window.onbeforeunload = flushPopoverClientLog;
    }
  } catch(e) {
  }
}

amznJQ.onReady('popover', function() {
    (function($) {
        enablePopoverLogging();
        var moreOptionsTimeoutText = 'Click to see more options';
        var loadingBarImg = '<img src="http://g-ecx.images-amazon.com/images/G/01/twister/images/narrowtimer_transparent._V192195438_.gif" width="195" height="16" border="0" />';
        var loadingBarImgSmall = '<img src="http://g-ecx.images-amazon.com/images/G/01/twister/images/loading-small._V192194710_.gif" width="14" height="14" border="0" />';
        var seeFullProductDetailsText = 'See full product details';
        
        var getAjaxURL = function(trigger, srParam) {
	    var endlessPopover = trigger.attr('endlessPopover');
	    var store = trigger.attr('store');
	    var merchantID = trigger.attr('merchantID');
	    if (merchantID == undefined) {
	      merchantID = "";
	    }
	    var dimKey = trigger.attr('dim');

            if(endlessPopover == 1) {
              var child  = $(trigger).attr('name').split('|')[0];
              var parent = $(trigger).attr('name').split('|')[1];
              var prtDiv = trigger.parents('div.product');
              var imgDiv = prtDiv.find('div.image');
              var searchImage = imgDiv.find('img.twisterRow').attr('src');

	      return '/gp/twister/search-popover/searchpopover-variation-matrix.html?id=' + parent + '&child=' + child 
		     + '&searchImage=' + searchImage + '&store=' + store + '&endlessPopover=1&qid=1361149854&sr=' 
		     + srParam + '&dimKey=' + dimKey + '&m=' + merchantID;
            } else {
              var parent = $(trigger).attr('name').split('|')[1];
              return '/gp/twister/search-popover/searchpopover-variation-matrix.html?id=' + parent + '&store=' + store 
                     + '&qid=1361149854&sr=' + srParam + '&dimKey=' + dimKey + '&m=' + merchantID;
            }
        }
        var srre = new RegExp("[\/?&]sr=([^&#]*)");
        $('a.searchpop_arrow, a.searchpop_text, div.container').each(function() {
            var trigger = $(this);
            var child = trigger.attr('name').split('|')[0];
            var parent = trigger.attr('name').split('|')[1];
	    var endlessPopover = $(trigger).attr('endlessPopover');
            var content = '<div class="grid results searchPopover" style="width: auto"><div class="result searchPopLeft" style="margin: 0px; padding-right: 15px; float: left; border-top: none; width: 200px;">';
            var prtDiv = trigger.parents('div.product');
            var imgDiv = prtDiv.find('div.image');
            var dataDiv = prtDiv.find('div.data');
            var dpLink = imgDiv.find('a:first').attr('href');
            if(endlessPopover == 1) {
              content += '<div class="image" id="popoverProductImage_' + parent + '">' + imgDiv.html() + '</div>';
              content += '<div class="data" id="popoverProductData_' + parent + '">' + dataDiv.html() + '</div>';
              content += '<br clear="all"><div id="SeeFullProductDetails" style="color:#E47911">&rsaquo; <a id="see_full_product_details" href="' + dpLink + '">'+ seeFullProductDetailsText + '</a></div>';
              content += '</div>'; // close div.searchPopLeft
              content += '<div class="ap_sub_content searchPopRight"><div class="loadingBarOuterDiv">' + loadingBarImgSmall + '</div></div>';
              content += '</div>'; // close div.defaultView
              content += '<div style="clear: both;"></div>';
            } else {
              content += '<div class="image">' + imgDiv.html() + '</div>';
              content += '<div class="data">' + dataDiv.html() + '</div>';
              content += '</div>'; // close div.searchPopLeft
              content += '<div class="ap_sub_content searchPopRight">' + loadingBarImg + '</div>';
              content += '</div>'; // close div.defaultView
              content += '<div style="clear: both;"></div>';
            }
            content = $(content);
            content.find('div[id^="searchql_child_' + child + '"]').hide();
            content.find('script').remove(); // remove script tags
            
            var srParam = dpLink.match(srre);
            if (srParam != null) {
              srParam = srParam[1];
            } else {
              srParam = "";
            }

            trigger.amazonPopoverTrigger({
              width: null,
              focusOnShow: false,
              localContent: content,
              showOnHover: trigger.hasClass('searchpop_arrow'),
              closeEventInclude: 'MOUSE_LEAVE',
              hoverShowDelay: 300,
              showCloseButton: false,
              destination: getAjaxURL(trigger, srParam),
              ajaxSlideDuration: 0,
              ajaxTimeout: 10000,
              ajaxErrorContent: function (XMLHttpRequest, textStatus) {            
                                  if(trigger.attr('endlessPopover') == 1) {
                                    if(textStatus == "timeout") {
                                      popoverTimeoutCount++;     
                                    } else {
                                      popoverOtherErrorsCount++;
                                    }  
                                  }
                                  return content.find('div >a:first').clone().text(moreOptionsTimeoutText); 
                                },
              location: 'over',
              align: 'img.twisterRow',
              locationElement: imgDiv.find('img.twisterRow'),
              clone: true,
              onFilled: function () {
                          if(trigger.attr('endlessPopover') == 1) {
                            // finding number of swatches
                            var popoverSwatchCount = $('div.ap_sub_content.searchPopRight').find('div.variationEndLess').find('ul').children().size();
                            // incrementing appropriate counter
                            if(popoverSwatchCount == 1) {
                              popoverSingleSwatchCount++;                
                            } else if(popoverSwatchCount > 1) {
                              popoverMultipleSwatchCount++; 
                            }
                          } 
                        }
            });
        });
    })(jQuery);
});
</script>



 


 

<script language="javascript">var cfimg = new Image();if (location.protocol == "http:") {if (window.addEventListener) {window.addEventListener("load", function() {cfimg.src = "http://cloudfront-labs.amazonaws.com/x.png";}, false);} else if (window.attachEvent) {window.attachEvent("onload", function() {cfimg.src = "http://cloudfront-labs.amazonaws.com/x.png";});}} </script> 

 
 



