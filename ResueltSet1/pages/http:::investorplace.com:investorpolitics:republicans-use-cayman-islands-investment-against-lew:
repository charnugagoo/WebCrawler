		<script type="text/javascript">
	/*<![CDATA[*/
			function getScrollY() {
		    scrOfY = 0;
		    if( typeof( window.pageYOffset ) == "number" ) {
		        scrOfY = window.pageYOffset;
		    } else if( document.body && ( document.body.scrollLeft || document.body.scrollTop ) ) {
		        scrOfY = document.body.scrollTop;
		    } else if( document.documentElement && ( document.documentElement.scrollLeft || document.documentElement.scrollTop ) ) {
		        scrOfY = document.documentElement.scrollTop;
		    }
		    return scrOfY;
	}
	
	function nr_fo_get_closed_cookie(){ 
		var NameOfCookie="nr_fo_closed";
		if (document.cookie.length > 0) { 
			begin = document.cookie.indexOf(NameOfCookie+"="); 
			if (begin != -1) { 
				begin += NameOfCookie.length+1; 
				end = document.cookie.indexOf(";", begin);
				if (end == -1) 
					end = document.cookie.length;
				return unescape(document.cookie.substring(begin, end)); 
			}
		}
		return "false"; 
	}
	
	function nr_fo_set_closed_cookie(value,domain) { 
		var NameOfCookie="nr_fo_closed";
		var ExpireDate = new Date ();
		ExpireDate.setTime(ExpireDate.getTime() + (7*24*60*60*1000));
		document.cookie = NameOfCookie + "=" + value + "; expires=" + ExpireDate.toGMTString()+"; path=/" + "; domain="+domain ;
	}
			value=nr_fo_get_closed_cookie();
		if(value=="false")
			nr_fo_closed=false;
		else
	  		nr_fo_closed=true;
		
		var nr_fo_hidden = true;
		jQuery(function($){$(window).scroll(function() {var lastScreen;lastScreen = getScrollY() + $(window).height() < ($("#nr_fo_bot_of_post").offset().top) - ($("#nr_fo_top_of_post").offset().top) ? false : true;if (lastScreen && !nr_fo_closed && getScrollY()!=0 && nRelate.flyout_show) {$(".nrelate_flyout").fadeIn("slow");nr_fo_hidden = false}else if (nr_fo_closed && lastScreen && getScrollY()!=0 && nRelate.flyout_show) {$("#nrelate_flyout_open").fadeIn("slow");nr_fo_hidden=false;}else if (!nr_fo_hidden && !nr_fo_closed) {$(".nrelate_flyout").fadeOut("slow");nr_fo_hidden = true;}else if (!nr_fo_hidden && nr_fo_closed) {$("#nrelate_flyout_open").fadeOut("slow");nr_fo_hidden = true;}});$("#nrelate_flyout_close").live("click",function() { $(".nrelate_flyout").fadeOut("slow"); $("#nrelate_flyout_open").fadeIn("slow"); nr_fo_closed = true; nr_fo_hidden = false; nr_fo_set_closed_cookie(true,"investorplace.com");});$("#nrelate_flyout_open").live("click",function() { $("#nrelate_flyout_open").fadeOut("slow"); $(".nrelate_flyout").fadeIn("slow");nr_fo_closed = false; nr_fo_hidden = false; nr_fo_set_closed_cookie(false,"investorplace.com");});});
		nRelate.domain = "investorplace.com";
		var entity_decoded_nr_url = jQuery('<div/>').html("http://api.nrelate.com/fow_wp/0.51.2/?tag=nrelate_flyout&keywords=Republicans+Use+Cayman+Islands+Investment+Against+Lew&domain=investorplace.com&url=http%3A%2F%2Finvestorplace.com%2Finvestorpolitics%2Frepublicans-use-cayman-islands-investment-against-lew%2F&nr_div_number=1").text();
		nRelate.getNrelatePosts(entity_decoded_nr_url);
	/*]]>*/
	</script>
